local tabButtonGroups = {
    Drawing = {"ActivateDrawing", "BoldDrawingButton"},
    ["Terror+"] = {"FullTerrorButton", "FullTerrorLoopButton"},
    Shapes = {"StarButton", "StarLoopButton", "CircleButton", "CircleLoopButton"},
    Fun = {"ExtremeRandomButton", "ExtremeRandomLoopButton"},
    PvP = {"HeadshotButton", "HeadshotLoopButton"},
    VETO = {"VetoButton", "VetoLoopButton", "VetoForAllButton", "VetoForAllLoopButton"},
    System = {"ActivateButton", "ExportButton", "ImportButton", "FullHavocButton", "ExtraTerrorButton", "ApostlesForAllButton", "HumanLoopButton", "MultiHumanLoopButton"}
}
for tab, btns in pairs(tabButtonGroups) do
    local secFrame = tabFrames[tab]
    local lay = Instance.new("UIListLayout",secFrame)
    lay.SortOrder = Enum.SortOrder.LayoutOrder
    lay.Padding = UDim.new(0,8)
    for _,bname in ipairs(btns) do
        local btn = Buttons[bname]
        if btn then
            btn.Parent = secFrame
        end
    end
end

local function showTab(tab)
    for t,frame in pairs(tabFrames) do
        frame.Visible = false
    end
    for t,btn in pairs(TabButtons) do
        btn.BackgroundColor3 = (t==tab) and Color3.fromRGB(59, 59, 105) or Color3.fromRGB(39,39,55)
        btn.TextColor3 = (t==tab) and Color3.fromRGB(250,230,80) or Color3.fromRGB(210,210,255)
    end
    tabFrames[tab].Visible = true
end
for tab,btn in pairs(TabButtons) do
    btn.MouseButton1Click:Connect(function() showTab(tab) end)
end
showTab("Drawing")

