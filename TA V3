	Script.AddCmd({"arrest"},"Arrests a player","<PLAYER,CRIMS>",function(Args)
		local saved = plr.Character:GetPivot()
		if Args[2] == "all" or Args[2] == "others" then
			for i,v in pairs(game:GetService("Players"):GetPlayers()) do
				if v and v~=plr and not table.find(Temp.Whitelisted,v.Name) and v.Team == game.Teams.Criminals or API.IsBadArea(v) and v ~=plr then
					repeat RStep:Wait()
						API.MoveTo(v.Character.Head.CFrame)
						Remotes.ArrestEvent(v)
					until v and v.Character and v.Character["Head"]:FindFirstChildOfClass("BillboardGui") or v.Character:FindFirstChildOfClass("Humanoid").Health <1
					for i =1,5 do
						game:GetService("ReplicatedStorage").meleeEvent:FireServer(v,{Name ='Crude Knife'})
					end
					wait(.1)
				end
			end
		elseif Args[2] == "crims" then
			for i,v in pairs(game:GetService("Players"):GetPlayers()) do
				if v and v~=plr and not table.find(Temp.Whitelisted,v.Name) and v.Team == game.Teams.Criminals then
					repeat RStep:Wait()
						API.MoveTo(v.Character.Head.CFrame)
						Remotes.ArrestEvent(v)
					until v.Character["Head"]:FindFirstChildOfClass("BillboardGui") or v.Character:FindFirstChildOfClass("Humanoid").Health <1
					for i =1,5 do
						game:GetService("ReplicatedStorage").meleeEvent:FireServer(v,{Name ='Crude Knife'})
					end
					wait(.1)
				end
			end
		else
			local Target = API.FindPlayer(tostring(Args[2]))
			if Target and Target.Team ~= game.Teams.Guards then
				if API.IsBadArea(Target) or game.Teams.Criminals then
					repeat RStep:Wait()
						API.MoveTo(Target.Character.Head.CFrame)
						Remotes.ArrestEvent(Target)
					until Target.Character["Head"]:FindFirstChildOfClass("BillboardGui") or Target.Character:FindFirstChildOfClass("Humanoid").Health <1 or Target.Team ~= game.Teams.Criminals and not API.IsBadArea(Target)
					for i =1,5 do
						game:GetService("ReplicatedStorage").meleeEvent:FireServer(Target,{Name ='Crude Knife'})
					end
					wait(.1)
				end
			elseif Target and Target.Team == game.Teams.Guards then
				API.Bring(Target,CFrame.new(-920.510803, 92.2271957, 2138.27002, 0, 0, -1, 0, 1, 0, 1, 0, 0)*CFrame.new(-4.5,2,-3))
				repeat task.wait() until Target.Team == game.Teams.Criminals
				repeat RStep:Wait()
					API.MoveTo(Target.Character.Head.CFrame)
					Remotes.ArrestEvent(Target)
				until Target.Character["Head"]:FindFirstChildOfClass("BillboardGui") or Target.Character:FindFirstChildOfClass("Humanoid").Health <1 or Target.Team == game.Teams.Guards
				for i =1,5 do
					game:GetService("ReplicatedStorage").meleeEvent:FireServer(Target,{Name ='Crude Knife'})
				end
				wait(.1)
			end
		end
		API.MoveTo(saved)
	end)
	Script.AddCmd({"criminal","crim"},"Change into Criminals Team or a person","<ME,PLAYER>",function(Args)
		if not Args[2] or Args[2] == "me" then
			API.SwitchTeam(game.Teams.Criminals,true)
		elseif Args[2] =="all" or Args[2] == "others" then
			for i,v in pairs(game:GetService("Players"):GetPlayers()) do
				if v and v.Team ~= game.Teams.Criminals then
					API.Bring(v,CFrame.new(-920.510803, 92.2271957, 2138.27002, 0, 0, -1, 0, 1, 0, 1, 0, 0)*CFrame.new(-4.5,2,-3))
					wait(.1)
				end
			end
		else
			local Target = API.FindPlayer(tostring(Args[2]))
			if Target then
				if Target.Team == game.Teams.Criminals then
					API.Notif("This player already is a criminal!","Error",2)
				else
					local lastpo = API.GetPos()
					API.Bring(Target,CFrame.new(-920.510803, 92.2271957, 2138.27002, 0, 0, -1, 0, 1, 0, 1, 0, 0)*CFrame.new(-4.5,2,-3))
					wait(.5)
					API.MoveTo(lastpo)
				end
			end
		end
	end)
	Script.AddCmd({"shield", "riotshield"}, "Gives you a shield", "<PLAYER/GUARD>", function(Args)
		table.remove(Args, 1) -- h1 why do u keep the command (dalloc) ... -- because its good (h1)
		if Args[1] == 'guard' then
			if not OwnsSWAT then
				API.Notif("You don't own the SWAT gamepass!")
				return
			end--//BY DALLOC
			local ply = game.Players.LocalPlayer
			local pos = ply.Character.Torso.CFrame
			local team = workspace.Remote.TeamEvent

			team:FireServer("Bright blue")
			wait(0.3)
			workspace.Remote.ItemHandler:InvokeServer({Position=ply.Character.Head.Position,Parent=workspace.Prison_ITEMS.giver["Riot Shield"]})
			local shield = ply.Backpack:WaitForChild("Riot Shield")

			shield.Parent = ply.Character
			spawn(function()
				for i = 1,5 do
					workspace.Remote.equipShield:FireServer(shield)
					wait()
				end
			end)
			team:FireServer("Bright orange")
			wait(0.5)
			ply.Character:PivotTo(pos)
		else
			local Target = API.FindPlayer(tostring(Args[1]))
			if Target and Target.Character and Target:FindFirstChild('Backpack') then
				local Shield = Target.Character:FindFirstChild('Riot Shield') or Target.Backpack:FindFirstChild('Riot Shield')
				if not Shield then
					API.Notif("This player doesn't have a shield!")
				else
					API.Notif("Found shield!")
					Remotes.EquipShield(Shield)
				end
			else
				local Shield = FindTool('Riot Shield')
				if not Shield then
					API.Notif("No shield found!")
				else
					API.Notif("Found shield!")
					Remotes.EquipShield(Shield)
				end
			end
		end
	end)
	Script.AddCmd({"keycard","key"},"Spawns a keycard","<ME,PLAYER>",function(Args)
		if not Args[2] or Args[2] == "me" then
			if not API.GuardsFull() then
				if game:GetService("Workspace")["Prison_ITEMS"].single:FindFirstChild("Key card") then
					repeat wait()
						workspace:FindFirstChild("Remote")['ItemHandler']:InvokeServer({Position = plr.Character.Head.Position,Parent = workspace.Prison_ITEMS.single:FindFirstChild("Key card", true)})
					until plr.Backpack:FindFirstChild("Key card") or plr.Character:FindFirstChild("Key card")
					return
				end
				local LastTeam,LastPos = plr.Team,API.GetPos()
				API.SwitchTeam(game.Teams.Guards)
				repeat task.wait(.15)
					if Settings.Unloaded then break end
					pcall(function()
						plr.Character:FindFirstChildOfClass("Humanoid"):ChangeState(15)
						wait(.25)
						API.SwitchTeam(game.Teams.Guards)
					end)
				until game:GetService("Workspace")["Prison_ITEMS"].single:FindFirstChild("Key card")
				API.SwitchTeam(LastTeam,true)
				wait(.1)
				repeat wait()
					workspace:FindFirstChild("Remote")['ItemHandler']:InvokeServer({Position = plr.Character.Head.Position,Parent = workspace.Prison_ITEMS.single:FindFirstChild("Key card", true)})
				until plr.Backpack:FindFirstChild("Key card") or plr.Character:FindFirstChild("Key card")
				API.MoveTo(LastPos)
			end
		else
			local Target = API.FindPlayer(tostring(Args[2]))
			if Target then
				if not API.GuardsFull() then
					local LastTeam,LastPos = plr.Team,API.GetPos()
					API.MoveTo(Target.Character.HumanoidRootPart.CFrame*CFrame.new(0,0,-2))
					API.SwitchTeam(game.Teams.Guards)
					repeat task.wait(.15)
						if Settings.Unloaded then break end
						pcall(function()
							API.MoveTo(Target.Character.HumanoidRootPart.CFrame*CFrame.new(0,0,-2))
							RStep:Wait()
							plr.Character:FindFirstChildOfClass("Humanoid"):ChangeState(15)
							wait(.25)
							API.SwitchTeam(game.Teams.Guards,nil,nil,nil,Target.Character.HumanoidRootPart.CFrame*CFrame.new(0,0,-2))
						end)
					until game:GetService("Workspace")["Prison_ITEMS"].single:FindFirstChild("Key card")
					API.SwitchTeam(LastTeam,true)
					wait(.1)
					API.MoveTo(LastPos)
				end
			end
		end
	end)
	Script.AddCmd({"whitelist","wl"},"Player will be ignored by the script","<PLAYER>",function(Args)
		local Target = API.FindPlayer(tostring(Args[2]))
		if Target then
			if not table.find(Temp.Whitelisted,Target.Name) then
				table.insert(Temp.Whitelisted,Target.Name)
			end
			API.Notif("Whitelisted "..Target.Name,"Information",2)
		end
	end)
	Script.AddCmd({"unwhitelist","unwl"},"Removes whitelist from a player","<PLAYER>",function(Args)
		local Target = API.FindPlayer(tostring(Args[2]))
		if Target then
			if table.find(Temp.Whitelisted,Target.Name) then
				table.remove(Temp.Whitelisted,table.find(Temp.Whitelisted,Target.Name))
			end
			API.Notif("Unwhitelisted "..Target.Name,"Information",2)
		end
	end)
	Script.AddCmd({"loopmkill","mlk"},"Loopkills a player by teleport","<PLAYER>",function(Args)
		local Target = API.FindPlayer(tostring(Args[2]))
		if Target then
			Temp.Loopmkilling = nil
			wait()
			Temp.Loopmkilling=Target
			API.Notif("Loopmkilling "..Target.Name,"Information",2)
			task.spawn(function()
				while wait() do
					task.wait()
					plr.Character:PivotTo(Target.Character:GetPivot())
					if Target and Target.Character and Target.Character:FindFirstChildOfClass("Humanoid").Health >1 and not Target.Character:FindFirstChildOfClass("ForceField") then
						for i =1,3 do
							game:GetService("ReplicatedStorage").meleeEvent:FireServer(Target,{Name ='Crude Knife'})
						end
					end
					if not Temp.Loopmkilling then
						break
					end
				end
			end)
		end
	end)
	Script.AddCmd({"unloopmkill","unmlk"},"Stops loopmkill","<PLAYER>",function(Args)
		local Target = API.FindPlayer(tostring(Args[2]))
		if Target then
			Temp.Loopmkilling=nil
		end
	end)
	Script.AddCmd({"loopkill","lk"},"Loopkills a player","<PLAYER,ALL,TEAM>",function(Args)
		if Args[2] == "all" or Args[2] == "others" then
			Toggles.LoopkillAll = true
			API.Notif("Loopkilling everyone","Information",2)
		elseif Args[2] == "guards" then
			Toggles.LoopkillGuards = true
			API.Notif("Loopkilling guards","Information",2)
		elseif Args[2] == "inmates" then
			API.Notif("Loopkilling inmates","Information",2)
			Toggles.LoopkillInmates = true
		elseif Args[2] == "criminals" then
			API.Notif("Loopkilling criminals","Information",2)
			Toggles.LoopkillCriminals = true
		else
			local Target = API.FindPlayer(tostring(Args[2]))
			if Target then
				if table.find(Temp.Whitelisted,Target.Name) then
					return API.Notif(Target.Name.." is whitelisted can't loopkill!","Information",2)
				end
				if not table.find(Temp.LoopKilling_Local,Target.Name) then
					table.insert(Temp.LoopKilling_Local,Target.Name)
					local TNAME = Target.Name
					while wait() do
						if table.find(Temp.LoopKilling_Local,Target.Name) and not Settings.Unloaded then
							local NewTarget = game:GetService("Players"):FindFirstChild(TNAME)
							repeat task.wait()  until NewTarget and NewTarget.Character and NewTarget.Character:FindFirstChildOfClass("Humanoid") and NewTarget.Character:FindFirstChildOfClass("Humanoid").Health >0 and not NewTarget.Character:FindFirstChildOfClass("ForceField")
							pcall(function() API.Kill_Player(NewTarget,{DontChangeTeam=true}) end)
						else
							break
						end 
					end
				end
				API.Notif("Loopkilling "..Target.Name,"Information",2)
			end
		end
	end)
	Script.AddCmd({"unloopkill","unlk"},"stops Loopkilling a player","<PLAYER,ALL,TEAM>",function(Args)
		if Args[2] == "all" or Args[2] == "others" then
			Toggles.LoopkillAll = false
			API.Notif("Unloopkilling everyone","Information",2)
		elseif Args[2] == "guards" then
			Toggles.LoopkillGuards = false
			API.Notif("Unloopkilling guards","Information",2)
		elseif Args[2] == "inmates" then
			API.Notif("Unloopkilling inmates","Information",2)
			Toggles.LoopkillInmates = false
		elseif Args[2] == "criminals" then
			API.Notif("Unloopkilling criminals","Information",2)
			Toggles.LoopkillCriminals = false
		else
			local Target = API.FindPlayer(tostring(Args[2]))
			if Target then
				if table.find(Temp.LoopKilling_Local,Target.Name) then
					table.remove(Temp.LoopKilling_Local,table.find(Temp.LoopKilling_Local,Target.Name))
					API.Notif("Stopped loopkilling "..Target.Name,"Information",2)
				end
			end
		end
	end)
	Script.AddCmd({"view"},"Views a player","<PLAYER>",function(Args)
		local Target = API.FindPlayer(tostring(Args[2]))
		if Target then
			Temp.ViewingPerson = nil
			wait()
			Temp.ViewingPerson = Target
			task.spawn(function()
				while wait() do
					pcall(function()
						workspace.CurrentCamera.CameraSubject = Target.Character:FindFirstChildOfClass("Humanoid")
					end)
					if not Temp.ViewingPerson then
						break
					end
				end
			end)
			API.Notif("Now viewing "..Target.Name,"Information",2)
		end
	end)
	Script.AddCmd({"unview"},"Stops viewing a player","<PLAYER>",function(Args)
		Temp.ViewingPerson = nil
		wait()
		workspace.CurrentCamera.CameraSubject = plr.Character:FindFirstChildOfClass("Humanoid")
		API.Notif("Stopped view","Information",2)
	end)
	Script.AddCmd({"speed"},"Sets your speed","<NUMBER>",function(Args)
		if Args[2] and tonumber(Args[2]) then
			plr.Character:FindFirstChildOfClass("Humanoid").WalkSpeed = tonumber(Args[2])
		end
	end)
	Script.AddCmd({"jumppower"},"Sets your jumppower","<NUMBER>",function(Args)
		if Args[2] and tonumber(Args[2]) then
			plr.Character:FindFirstChildOfClass("Humanoid").JumpPower = tonumber(Args[2])
		end
	end)
	Script.AddCmd({"prefix"},"Sets a new prefix","<NEW PREFIX>",function(Args)
		if Args[2] then
			Settings.Prefix = tostring(Args[2])
			CommandBar.Text = ""
			wait()
			for i,v in pairs(CommandsList:GetChildren()) do
				if v and v:IsA("TextButton") then
					v:FindFirstChildOfClass("TextLabel").Text = Settings.Prefix..v:FindFirstChildOfClass("TextLabel").Text:sub(2,#v:FindFirstChildOfClass("TextLabel").Text)
				end
			end
		else
			API.Notif("You need a new prefix! ex: !prefix ;")
		end
	end)
	----------------------TOGGLES
	Script.AddCmd({"supergod"},"Turns on AntiArrest,Shootback,AntiPunch,AntiTase,Antibring,AntiFling","<ON/OFF>",function(Args)
		API.ToggleItem("SuperGod",Args[2])
		if Toggles.SuperGod then
			Toggles.AntiArrest = true
			Toggles.ShootBack = true
			Toggles.AntiPunch = true
			do --// ANTIPUNCH
				Temp.ConnectionsPUNCH = {}
				for i,v in pairs(game:GetService("Players"):GetPlayers()) do
					if v and v~=plr then
						task.spawn(function()
							API.AntiPunchC(v)
						end)
						Temp.ConnectionsPUNCH[i] = v.CharacterAdded:Connect(function(c)
							repeat task.wait() until c and c:FindFirstChildOfClass("Humanoid")
							if Toggles.AntiPunch then
								if Toggles.AntiPunch then
									API.AntiPunchC(v)
								end
							end
						end)
					end
				end
			end
			Toggles.AntiBring = true
			Toggles.AntiFling = true
			Toggles.AntiTase = true
			wait(.1)
			API.SwitchTeam(game.Players.LocalPlayer.Team, true,nil,true)
		else
			Toggles.AntiFling = false
			Toggles.AntiBring = false
			Toggles.AntiArrest = false
			Toggles.ShootBack = false
			Toggles.AntiPunch = false
			do --// Antipunch
				if Temp.ConnectionsPUNCH then
					for i,v in pairs(Temp.ConnectionsPUNCH) do
						if v then
							v:Disconnect()
						end
					end
				end
				Temp.ConnectionsPUNCH = nil
			end
			Toggles.AntiTase = false
		end
	end)
	Script.AddCmd({"carspam"},"LoopBrings cars to you","<ON/OFF>",function(Args)
		API.ToggleItem("CarSpam",Args[2])
		local Older = API.GetPos()
		local OldPos = plr.Character:GetPrimaryPartCFrame()
		while wait(.1) do
			if not Toggles.CarSpam or Settings.Unloaded then break end
			pcall(function()
				API.UnSit()
				local CarSpawner = nil
				for i,v in pairs(workspace.Prison_ITEMS.buttons:GetChildren()) do
					if v and v.Name:lower():find("car ") and not v["Car Spawner"].deb.Value then
						CarSpawner = v
					end
				end
				API.UnSit()
				game:GetService("Players").LocalPlayer.Character:SetPrimaryPartCFrame(CarSpawner["Car Spawner"].CFrame*CFrame.new(0,2,0))
				wait()
				local car = nil
				task.spawn(function()
					car = game:GetService("Workspace").CarContainer.ChildAdded:Wait()
				end)
				if not car then
					repeat wait(.05)
						API.UnSit()
						workspace.Remote.ItemHandler:InvokeServer(CarSpawner["Car Spawner"])
					until car
				end
				repeat task.wait() until car:FindFirstChild("RWD") and car:FindFirstChild("Body") and car:FindFirstChild("Body"):FindFirstChild("VehicleSeat")
				car.PrimaryPart = car.RWD
				car:SetPrimaryPartCFrame(API.GetPos())
				API.UnSit()
				game:GetService("Players").LocalPlayer.Character:SetPrimaryPartCFrame(OldPos)
				local Done = false
				car.Body.VehicleSeat:Sit(game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid"))
				repeat 
					API.UnSit()
					game:GetService("RunService").RenderStepped:Wait()
					car:SetPrimaryPartCFrame(OldPos)
					game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame =CFrame.new(car.Body.VehicleSeat.Position)
					car.Body.VehicleSeat:Sit(game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid"))
					if game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid").Sit == true then
						Done = true
					end
				until Done
				wait(.3)
				API.UnSit()
				API.MoveTo(Older)
			end)
		end
	end)
	Script.AddCmd({"silentaim"},"Don't have to aim to kill","<ON/OFF>",function(Args)
		API.ToggleItem("SilentAim",Args[2])
		if Toggles.SilentAim then
			local Target = nil
			local MAX_RANGE = 999999
			local line = nil
			local Auto = {
				"ak-47",
				"m4a1",
			}
			if Drawing then --// doesnt have to support drawning the script will work anyways
				line=Drawing.new("Line")
				line.Visible = false
				line.Color = Color3.new(1, 0, 0)
				line.Thickness = 2
				Temp.Line = line
			end
			local function BehindWall(target)
				if not target.Character or not target.Character:FindFirstChild("HumanoidRootPart") then
					return false
				end
				local origin = workspace.CurrentCamera.CFrame.Position
				local dir = (target.Character.HumanoidRootPart.Position-origin).unit*(target.Character.HumanoidRootPart.Position - origin).magnitude
				local ray = Ray.new(origin,dir)
				local hit = workspace:FindPartOnRay(ray, plr.Character)
				if hit and hit:IsDescendantOf(target.Character) then
					return false
				end
				return true
			end

			local function MouseClosest()
				local closestPlayer = nil
				local shortestDistanceToMouse = math.huge
				for i,player in pairs(game:GetService("Players"):GetPlayers()) do
					if player ~= plr and player.Team ~= plr.Team and player.Character and player.Character:FindFirstChild("HumanoidRootPart") and player.Character:FindFirstChildOfClass("Humanoid").Health>0 and not player.Character:FindFirstChildOfClass("ForceField") and not table.find(Temp.Whitelisted,player.Name) then
						local characterPart = player.Character.HumanoidRootPart
						local distanceToLocalPlayer = (characterPart.Position -plr.Character.HumanoidRootPart.Position).Magnitude
						if distanceToLocalPlayer <= MAX_RANGE then
							local vector, onScreen = Camera:WorldToScreenPoint(characterPart.Position)
							if onScreen and not BehindWall(player) then
								local mousePosition = Vector2.new(plr:GetMouse().X, plr:GetMouse().Y)
								local distanceToMouse = (Vector2.new(vector.X, vector.Y)- mousePosition).Magnitude
								if distanceToMouse < shortestDistanceToMouse then
									closestPlayer = player
									shortestDistanceToMouse = distanceToMouse
								end
							end
						end
					end
				end

				return closestPlayer
			end

			local function UpdateLine(targetPlayer)
				if targetPlayer and line and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
					local startPosition = Camera:WorldToScreenPoint(plr.Character.HumanoidRootPart.Position)
					local endPosition = Camera:WorldToScreenPoint(targetPlayer.Character.HumanoidRootPart.Position-Vector3.new(0,1,0))
					line.From = Vector2.new(startPosition.X, startPosition.Y)
					line.To = Vector2.new(endPosition.X, endPosition.Y)
					line.Visible = true
				else
					line.Visible = false
				end
			end
			Temp.SilentAim = game:GetService("RunService").RenderStepped:Connect(function()
				local closestPlayer = MouseClosest()
				if closestPlayer then
					Target=closestPlayer
					if line then
						UpdateLine(closestPlayer)
					end
				else
					if line then
						UpdateLine(nil)
					end
				end
			end)
			local Held = false
			Temp.Input = game:GetService("UserInputService").InputBegan:Connect(function(Key,c)
				if not c then
					if Key.UserInputType == Enum.UserInputType.MouseButton1 and not c then
						Held = true
					end
				end
			end)	
			Temp.Input2 = game:GetService("UserInputService").TouchLongPress:Connect(function(a,State)
				if State ~=nil and not game:GetService("UserInputService"):GetFocusedTextBox() then
					Held = State
				end
			end)
			Temp.Input = game:GetService("UserInputService").InputEnded:Connect(function(Key,c)
				if Key.UserInputType == Enum.UserInputType.MouseButton1  and not c then
					Held = false
				end
				if not c then
					if Key.UserInputType == Enum.UserInputType.MouseButton1 or Key.UserInputType == Enum.UserInputType.Touch then
						if plr.Character:FindFirstChildOfClass("Tool") and plr.Character:FindFirstChildOfClass("Tool"):FindFirstChild("GunStates")  then
							if plr.Team ~= Target.Team and Target then
								local Tool = plr.Character:FindFirstChildOfClass("Tool")
								if table.find(Auto,Tool.Name:lower()) then
									return
								end
								if Tool.Name == "Remington 870" then
									game:GetService("ReplicatedStorage"):WaitForChild("ShootEvent"):FireServer({{
										["RayObject"] = Ray.new(Vector3.new(), Vector3.new()),
										["Distance"] = 0,
										["Cframe"] = CFrame.new(),
										["Hit"] = Target.Character:FindFirstChild("Head"),
									},{
										["RayObject"] = Ray.new(Vector3.new(), Vector3.new()),
										["Distance"] = 0,
										["Cframe"] = CFrame.new(),
										["Hit"] = Target.Character:FindFirstChild("Head"),
									}}, Tool)
								else
									game:GetService("ReplicatedStorage"):WaitForChild("ShootEvent"):FireServer({{
										["RayObject"] = Ray.new(Vector3.new(), Vector3.new()),
										["Distance"] = 0,
										["Cframe"] = CFrame.new(),
										["Hit"] = Target.Character:FindFirstChild("Head"),
									}}, Tool)
								end
								game:GetService("ReplicatedStorage").ReloadEvent:FireServer(Tool)
							end
						end
					end
				end
			end)
			while wait(.1) do
				local Tool = plr.Character:FindFirstChildOfClass("Tool")
				if Held and Tool then
					if table.find(Auto,Tool.Name:lower()) and Target then
						game:GetService("ReplicatedStorage"):WaitForChild("ShootEvent"):FireServer({{
							["RayObject"] = Ray.new(Vector3.new(), Vector3.new()),
							["Distance"] = 0,
							["Cframe"] = CFrame.new(),
							["Hit"] = Target.Character:FindFirstChild("Head"),
						}}, Tool)
						game:GetService("ReplicatedStorage").ReloadEvent:FireServer(Tool)
					end
				end
				if not Toggles.SilentAim then
					break
				end
			end
		else
			Temp.Input:Disconnect()
			Temp.SilentAim:Disconnect()
			Temp.Input2:Disconnect()
			if Temp.Line then
				Temp.Line.Visible = false
			end
		end
	end)

	Script.AddCmd({"joinlogs"},"Tells you who joined and left the server","<ON/OFF>",function(Args)
		API.ToggleItem("JoinLogs",Args[2])
		if Toggles.JoinLogs then
			Temp.joinning = game:GetService("Players").PlayerAdded:Connect(function(a)
				game.StarterGui:SetCore("ChatMakeSystemMessage",  { Text = "[JOIN LOGS]: "..a.Name.." has joined the game!", Color = Color3.fromRGB(16, 243, 255), Font = Enum.Font.SourceSansBold, FontSize = Enum.FontSize.Size24 } )
			end)
			Temp.joinning2 = game:GetService("Players").PlayerRemoving:Connect(function(a)
				game.StarterGui:SetCore("ChatMakeSystemMessage",  { Text = "[JOIN LOGS]: "..a.Name.." has left the game!", Color = Color3.fromRGB(50, 14, 255), Font = Enum.Font.SourceSansBold, FontSize = Enum.FontSize.Size24 } )
			end)
		else
			pcall(function()
				Temp.joinning2:Disconnect()
				Temp.joinning:Disconnect()
			end)
		end
	end)
	Script.AddCmd({"earrape"},"Spams sounds","<ON/OFF>",function(Args)
		API.ToggleItem("EarRape",Args[2])
		if Toggles.EarRape then
			while wait(.1) do
				for i,v in pairs(workspace:GetDescendants()) do
					if v:IsA("Sound") then
						v:Play()
					end
				end
				if not Toggles.EarRape or Settings.Unloaded then
					break
				end
			end
		end
	end)

	Script.AddCmd({"noclip"},"Walk through walls","<ON/OFF>",function(Args)
		API.ToggleItem("Noclip",Args[2])
		if Toggles.Noclip then
			Temp.Noclipping = game:GetService("RunService").Stepped:Connect(function()
				for i,v in pairs(plr.Character:GetDescendants()) do
					if v and v:IsA("Part") or v:IsA("MeshPart") and v.CanCollide and not Settings.Unloaded then
						v.CanCollide = false
					end
				end
			end)
		else
			if Temp.Noclipping then
				Temp.Noclipping:Disconnect()
				Temp.Noclipping=nil
			end
		end
	end)
	Script.AddCmd({"esp"},"See where every player is","<ON/OFF>",function(Args)
		API.ToggleItem("Esp",Args[2])
		if Toggles.Esp then
			if workspace:FindFirstChild("HIGHLIGHTS_1000") then
				return 
			end
			local Group = Instance.new("Model",workspace)
			Group.Name = "HIGHLIGHTS_1000"
			local esp =Instance.new("Highlight",Group)
			esp.FillColor = Color3.new(1, 1, 1)
			Temp.ESP = esp
			local function EspToggle(v)
				if not Temp.EspConnection  then
					return
				end
				task.spawn(function()
					if v.Character and v.Character:FindFirstChild("Head") then
						if v.Character.Parent == Group then
							return
						end
						v.Character.Archivable = true
						local v1 = Instance.new("BillboardGui",v.Character:FindFirstChild("Head"))
						local v2 = Instance.new("TextLabel",v1)
						table.insert(Temp.EspTable, v1)
						table.insert(Temp.EspTable, v2)
						v1.Adornee = v.Character.Head
						v1.Name = "123123123123"
						v1.Size = UDim2.new(0, 100, 0, 150)
						v1.StudsOffset = Vector3.new(0, 2, 0)
						v1.AlwaysOnTop = true
						v2.BackgroundTransparency = 1
						v2.TextColor3 = v.TeamColor.Color
						v2.TextStrokeTransparency = 0
						v2.TextYAlignment = Enum.TextYAlignment.Bottom
						v2.Position = UDim2.new(0, 0, 0, -50)
						v2.Size = UDim2.new(0, 120, 0, 120)
						v2.Font = Enum.Font.SourceSansBold
						v2.TextSize = 15
						v2.Text = v.Name
						v.Character.Parent = Group
					end
				end)
			end
			Temp.EspConnection=true
			repeat wait(.5)
				for i,v in pairs(game:GetService("Players"):GetPlayers()) do
					if v and v~= game:GetService("Players").LocalPlayer and not Settings.Unloaded then
						EspToggle(v)
					end
				end
				for i,v in pairs(Temp.EspTable) do
					if v.Parent == nil or Settings.Unloaded then
						v:Destroy()
					end
				end
				if Settings.Unloaded then
					Temp.ESP:Destroy()
					Temp.EspConnection = nil
					wait(.1)
					for i,v in pairs(workspace:FindFirstChild("HIGHLIGHTS_1000"):GetChildren()) do
						v.Parent = workspace
					end
					for i,v in pairs(Temp.EspTable) do
						v:Destroy()
					end
					workspace:FindFirstChild("HIGHLIGHTS_1000"):Destroy()
					break
				end
			until Temp.EspConnection == nil 
		else
			if workspace:FindFirstChild("HIGHLIGHTS_1000") then
				Temp.ESP:Destroy()
				Temp.EspConnection = nil
				wait(.1)
				for i,v in pairs(workspace:FindFirstChild("HIGHLIGHTS_1000"):GetChildren()) do
					v.Parent = workspace
				end
				for i,v in pairs(Temp.EspTable) do
					v:Destroy()
				end
				workspace:FindFirstChild("HIGHLIGHTS_1000"):Destroy()

			end
		end
	end)
	Script.AddCmd({"loopopendoors"},"Loop opendoors","<ON/OFF>",function(Args)
		API.ToggleItem("loopopendoors",Args[2])
		if Toggles.loopopendoors then
			while wait(3) do
				if firetouchinterest then
					if plr.Team ~= game.Teams.Guards then
						API.SwitchTeam(game.Teams.Guards)
					end
					wait(.7)
					for i,v in pairs(game:GetService("Workspace").Doors:GetChildren()) do
						if v then
							if v:FindFirstChild("block") and v:FindFirstChild("block"):FindFirstChild("hitbox") then
								firetouchinterest(plr.Character.HumanoidRootPart,v.block.hitbox,0)
								firetouchinterest(plr.Character.HumanoidRootPart,v.block.hitbox,1)
							end
						end
					end
					if Settings.Unloaded or not Toggles.loopopendoors then
						break
					end 
				end
			end
		end
	end)
	Script.AddCmd({"antifling"},"Prevents people from flinging you","<ON/OFF>",function(Args)
		API.ToggleItem("AntiFling",Args[2])
	end)
	Script.AddCmd({"antishield"},"Deletes all shields","<ON/OFF>",function(Args)
