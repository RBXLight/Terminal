	Script.AddCmd({"spamtoilet"},"Spams 5k toilets",nil,function(Args)
		local hammer = { Parent = plr.Backpack, Name = "Hammer"}
		local cells = workspace.Prison_Cellblock
		local cells_a = cells.Cells_A
		local cells_b = cells.Cells_B
		local function destroyToilet(cell,amt)
			if cell and cell:findFirstChild("Toilet") then
				for i = 1,amt or 1 do
					game:GetService("ReplicatedStorage").meleeEvent:FireServer(cell.Toilet, hammer)
				end
			end
		end
		for i = 1,8 do
			local cella, cellb = cells_a["cell"..i], cells_b["cell"..i]
			destroyToilet(cella, 5000)
			destroyToilet(cellb, 5000)
		end
	end)
	]]
