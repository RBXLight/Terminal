function Update_Loading_Percent(To)
	Tween(LoadingInner,"Size",UDim2.new(To/100,0,1,0),.2)
	if To == 100 then
		wait(1)
		LoadingGUI_TIGER:Destroy()
		TIGERADMIN.Enabled = true
	end
end
Update_Loading_Percent(10)
