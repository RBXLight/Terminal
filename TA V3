	end)
	Script.AddCmd({"yard"},"Teleports you to that location","<ME,PLAYER>",function(Args)
		local Pos = CFrame.new(792.50885, 97.9999237, 2467.05664, 0.999635994, 2.4151424e-08, -0.0269802082, -2.5547001e-08, 1, -5.13812246e-08, 0.0269802082, 5.20517851e-08, 0.999635994)
		if Args[2] then
			local Target = API.FindPlayer(tostring(Args[2]))
			if Target then
				API.Bring(Target,Pos)
			end
		else
			API.MoveTo(Pos)
		end
	end)
	Script.AddCmd({"tower"},"Teleports you to that location","<ME,PLAYER>",function(Args)
		local Pos = CFrame.new(821.8330078125, 130.03990173339844, 2570.027587890625)
		if Args[2] then
			local Target = API.FindPlayer(tostring(Args[2]))
			if Target then
				API.Bring(Target,Pos)
			end
		else
			API.MoveTo(Pos)
		end
	end)
	Script.AddCmd({"gate"},"Teleports you to that location","<ME,PLAYER>",function(Args)
		local Pos = CFrame.new(501.7165222167969, 125.0399169921875, 2304.559326171875)
		if Args[2] then
			local Target = API.FindPlayer(tostring(Args[2]))
			if Target then
				API.Bring(Target,Pos)
			end
		else
			API.MoveTo(Pos)
		end
	end)
	Script.AddCmd({"crimbase","criminalbase","cbase"},"Teleports you to that location","<ME,PLAYER>",function(Args)
		local Pos = CFrame.new(-864.0189819335938, 94.47602844238281, 2065.45947265625)
