API.Notif = function(Text,Title,Time)
	task.spawn(function()
		local c = Notification:Clone()
		c.Parent = NotificationHolder
		c.InnerDisc.TextInput.Text = Text or "Uh oh, looks like the devs forgot to put something here"
		c.InnerDisc.TopNotif.NotifTitle.Text = Title or "Notification"
		for i,v in pairs(c:GetDescendants()) do
			pcall(function()
				v.BackgroundTransparency = 1
			end)
			pcall(function()
				v.TextTransparency = 1
			end)
			pcall(function()
				v.Transparency = 1
			end)
		end
		c.Visible = true
		Tween(c,"BackgroundTransparency",0.75,1)
		Tween(c.InnerDisc,"BackgroundTransparency",0.25,1)
		Tween(c.InnerDisc.TopNotif,"BackgroundTransparency",0.95,1)
		Tween(c.InnerDisc.TopNotif.NotifTitle,"TextTransparency",0,1)
		Tween(c.InnerDisc.TextInput,"TextTransparency",0,1)
		Tween(c.UIStroke,"Transparency",0,1)
		wait(Time or 3.2)
		Tween(c,"BackgroundTransparency",1,1)
		Tween(c.InnerDisc,"BackgroundTransparency",1,1)
		Tween(c.InnerDisc.TopNotif,"BackgroundTransparency",1,1)
		Tween(c.InnerDisc.TopNotif.NotifTitle,"TextTransparency",1,1)
		Tween(c.InnerDisc.TextInput,"TextTransparency",1,1)
		Tween(c.UIStroke,"Transparency",1,1)
		wait(1.05)
		c:Destroy()
	end)
end
API.Dragify = function(Frame)
	task.spawn(function()
		local dragToggle = nil
		local dragInput = nil
		local dragStart = nil
		local dragPos = nil
		local Position = nil
		local Delta = nil
		local startPos = nil
		local function updateInput(input)
			Delta = input.Position - dragStart
			Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
			game:GetService("TweenService"):Create(Frame, TweenInfo.new(0.15), {Position = Position}):Play()
		end
		Frame.InputBegan:Connect(function(input)
			if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and game:GetService("UserInputService"):GetFocusedTextBox() == nil then
				dragToggle = true
				dragStart = input.Position
				startPos = Frame.Position
				input.Changed:Connect(function()
					if input.UserInputState == Enum.UserInputState.End then
						dragToggle = false
					end
				end)
			end
		end)
		Frame.InputChanged:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
				dragInput = input
			end
		end)
		game:GetService("UserInputService").InputChanged:Connect(function(input)
			if input == dragInput and dragToggle then
				updateInput(input)
			end
		end)
	end)
end
API.Dragify(Main)
API.Dragify(Open)
--\\ Tables
local SavePos = {
	CrimPad = workspace["Criminals Spawn"]:FindFirstChild("SpawnLocation").CFrame
}
local ChatCD = 0

API.FindPlayer = function(String,Admin)
	if not String then
		return nil
	end
	if String:lower() == "random" then
		local RandomDude = game:GetService("Players"):GetChildren()[math.random(1,#game:GetService("Players"):GetChildren())]
		if RandomDude and RandomDude == plr then
			API.FindPlayer(String,Admin)
		end
		return RandomDude
	end
	if String:lower() == "all" then
		API.Notif("This command most likely doesnt support 'all'")
		return
	end
	if String:lower() == "me" then
		if Admin then
			return Admin
		else
			return plr
		end
	end
	String = String:gsub("%s+", "")
	for _, v in pairs(game:GetService("Players"):GetPlayers()) do
		if v.Name:lower():match("^" .. String:lower()) or v.DisplayName:lower():match("^" .. String:lower()) then
			return v
		end
	end
	API.Notif("No player under the name of '"..String.."' has been found.","Error")
	return nil
end
API.UnSit = function()
	game:GetService("Players").LocalPlayer.Character:WaitForChild("Humanoid").Sit = false
end
API.MoveTo = function(To,Amount)
	local Amount = 5
	if Lighting.ClockTime < 6 or Lighting.ClockTime > 22 then 
		Amount = 11
	end
	for i,v in ipairs(plr.Character:GetDescendants()) do
		pcall(function()
			v.Velocity = Vector3.new(0, 0, 0)
		end)
	end
	for i = 1, Amount do
		API.UnSit()
		plr.Character:FindFirstChild("HumanoidRootPart").CFrame = To
		game:GetService("RunService").Stepped:Wait()
	end
end
API.GetPos = function()
	return plr.Character:GetPrimaryPartCFrame()
end
local Get = {
	Root = function(arg)
		if arg then
			return arg.Character and arg.Character:FindFirstChild("HumanoidRootPart") or arg.Character:GetPivot()
		else
			return plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") or plr.Character:WaitForChild("HumanoidRootPart", 1)
		end
	end;
	Human = function(arg)
		if arg then
			return arg.Character:FindFirstChild("HumanoidRootPart")
		else
			return plr.Character:FindFirstChild("HumanoidRootPart")
		end
	end;
	Ping = function(ConvertToHuman)
		return ConvertToHuman and game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValue() or game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValue() / 1000
	end;
};
API.ToggleValue = function(Value)
	local ItemHandler = workspace:WaitForChild('Remote'):WaitForChild('ItemHandler')
	local Gate = workspace.Prison_ITEMS.buttons["Prison Gate"]
	if (not Get.Root()) then
		API.Notif('No root available.')
		return
	end
	local Table = { Parent = Gate, Position = Get.Root().Position, isActive = Value }
	task.spawn(function()
		ItemHandler:InvokeServer(Table)
	end)
end

Main.MouseLeave:Connect(function() DescripTEXT.Visible = false end)

local Script = {
	AddCmd = function(Name,Desc,Args,Call)
		Temp.cmds = Temp.cmds+1
		local CommandBUTTON = TemplateCommandButton:Clone()
		CommandBUTTON.Visible = true
		CommandBUTTON.Parent = CommandsList
		CommandBUTTON:FindFirstChildOfClass("TextLabel").Text = Settings.Prefix..Name[1].." "..(Args or "")
		for i,v in pairs(Name) do
			CommandsHolder[v:lower()] = Call
		end
		CommandBUTTON.MouseButton1Click:Connect(function()
			CommandBar:CaptureFocus()
			CommandBar.Text = Name[1]
			CommandBar.CursorPosition = 100
		end)
		CommandBUTTON.MouseEnter:Connect(function()
			DescripTEXT.Text = Desc
			DescripTEXT.Visible = true
			Tween(CommandBUTTON,"BackgroundTransparency",.75,.3)
		end)
		CommandBUTTON.MouseLeave:Connect(function()
			Tween(CommandBUTTON,"BackgroundTransparency",0.9,.3)
		end)
	end;
	RunCommand = function(Name,Args,Chat,Raw)
		Name=Name:lower()
		if Settings.Unloaded then
			return
		end
		if Chat and tick() - ChatCD < .75 then
			return
		end
		if string.sub(Name,1,1) == Settings.Prefix then
			Name = string.sub(Name,2,#Name)
		end
		ChatCD = tick()
		if CommandsHolder[Name] ~=nil then
			task.spawn(function()
				local Ran,Err = pcall(function()
					CommandsHolder[Name](Args,Raw)
				end)
				if Err then
					local FormattedError = ("CMD ERROR: '%s' | CMD: '!%s' | Copied to clipboard."):format(Err,Name)
					API.Notif(FormattedError,"DEBUGGER",10)
					if setclipboard then
						setclipboard(FormattedError)
					end
				else
					if Settings.Debug then
						API.Notif(("RAN COMMAND: !%s | NO ERRORS DETECTED"):format(Name))
					end
				end
			end)
			return true
		else
			return nil
		end
	end;
}
API.GetGun = function(Item)
	task.spawn(function()
		local isAlreadyThere = plr.Backpack:FindFirstChild(Item) or plr.Character:FindFirstChild(Item)
		if isAlreadyThere then
			return isAlreadyThere
		else
			pcall(function()
				for i =1,4 do
					task.spawn(function()
						workspace:FindFirstChild("Remote")['ItemHandler']:InvokeServer({Position = plr.Character.Head.Position,Parent = workspace.Prison_ITEMS:FindFirstChild(Item, true)})
					end)
				end
			end)
		end
	end)
end
