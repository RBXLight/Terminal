		if not API.GuardsFull() then
			API.SwitchTeam(game.Teams.Guards)
			wait()
			local oldp,oldt = API.GetPos(),plr.Team
			for i =1,25 do
				task.wait(.1)
				if Settings.Unloaded then break end
				pcall(function()
					plr.Character:FindFirstChildOfClass("Humanoid"):ChangeState(15)
					wait(.2)
					API.MoveTo(CFrame.new(916.7511596679688, 99.98997497558594, 2381.497314453125))
				end)
			end
			wait()
			API.MoveTo(oldp)
			wait()
			API.SwitchTeam(oldt)
		end
	end)
