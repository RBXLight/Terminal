	----------------------GUN MODIFICATIONS
	Script.AddCmd({"minigun"},"Gives you a (real) minigun",nil,function(Args)
		local PreferGun = "AK-47"
		if OwnsSWAT then PreferGun = "M4A1" API.GetGun("M4A1") else API.GetGun("AK-47") end 
		
		plr.Backpack:WaitForChild(PreferGun)
		local Tool = plr.Backpack:FindFirstChildOfClass("Tool")
		if not Tool:FindFirstChild("GunStates") then
			return API:Notif("Needs to be a gun!",false)
		end
		pcall(function()
			if not HasRequire then
				local cc = require(Tool.GunStates)
				cc["Damage"] = 9e9
				cc["FireRate"] = 0.001
				cc["Range"] = math.huge
				cc["MaxAmmo"] = math.huge
				cc["StoredAmmo"] = math.huge
				cc["AmmoPerClip"] = math.huge
				cc["CurrentAmmo"] = math.huge
				if Tool.Name ~= "Remington 870" then
					cc["Bullets"] = 1
					cc["AutoFire"] = true
				end
			end
		end)
		table.insert(ReloadingGuns, Tool)
		plr.Character:FindFirstChildOfClass("Humanoid"):UnequipTools()
		API.GetGun("Remington 870")
		plr.Backpack:WaitForChild("Remington 870")
		for i,v in pairs(plr.Backpack:GetChildren()) do
			if v and v:FindFirstChildOfClass("ModuleScript") then
				v.Grip = v.Grip
				v.Grip=v.Grip*CFrame.Angles(0,0,math.rad(180))
			end
		end
		plr.Backpack:WaitForChild("Remington 870").Parent = plr.Character
		wait()
		plr.Backpack:WaitForChild(PreferGun).Parent = plr.Character
	end)
	if HasRequire then
		Script.AddCmd({"autofire"},"Mods your gun",nil,function(Args)
			if plr.Character:FindFirstChildOfClass("Tool") then
				local Tool = plr.Character:FindFirstChildOfClass("Tool")
				if not Tool:FindFirstChild("GunStates") then
					return API:Notif("Needs to be a gun!",false)
				end
				local cc = require(Tool.GunStates)
				cc["AutoFire"] = true
				table.insert(ReloadingGuns, Tool)
				plr.Character:FindFirstChildOfClass("Humanoid"):UnequipTools()
				Tool.Parent = plr.Character
			else
				API.Notif("You need to hold the tool you want to mod!")
			end
		end)
		Script.AddCmd({"fastfire"},"Mods your gun",nil,function(Args)
			if plr.Character:FindFirstChildOfClass("Tool") then
				local Tool = plr.Character:FindFirstChildOfClass("Tool")
				if not Tool:FindFirstChild("GunStates") then
					return API:Notif("Needs to be a gun!",false)
				end
				local cc = require(Tool.GunStates)
				cc["FireRate"] = 0.001
				table.insert(ReloadingGuns, Tool)
				plr.Character:FindFirstChildOfClass("Humanoid"):UnequipTools()
				Tool.Parent = plr.Character
			else
				API.Notif("You need to hold the tool you want to mod!")
			end
		end)
		Script.AddCmd({"infammo"},"Mods your gun",nil,function(Args)
			if plr.Character:FindFirstChildOfClass("Tool") then
				local Tool = plr.Character:FindFirstChildOfClass("Tool")
				if not Tool:FindFirstChild("GunStates") then
					return API:Notif("Needs to be a gun!",false)
				end
				local cc = require(Tool.GunStates)
				cc["MaxAmmo"] = math.huge
				cc["StoredAmmo"] = math.huge
				cc["AmmoPerClip"] = math.huge
				cc["CurrentAmmo"] = math.huge
				table.insert(ReloadingGuns, Tool)
				plr.Character:FindFirstChildOfClass("Humanoid"):UnequipTools()
				Tool.Parent = plr.Character
			else
				API.Notif("You need to hold the tool you want to mod!")
			end
		end)

		Script.AddCmd({"modgun"},"Mods your gun",nil,function(Args)
			if plr.Character:FindFirstChildOfClass("Tool") then
				local Tool = plr.Character:FindFirstChildOfClass("Tool")
				if not Tool:FindFirstChild("GunStates") then
					return API:Notif("Needs to be a gun!",false)
				end
				local cc = require(Tool.GunStates)
				cc["Damage"] = 9e9
				cc["FireRate"] = 0.001
				cc["Range"] = math.huge
				cc["MaxAmmo"] = math.huge
				cc["StoredAmmo"] = math.huge
				cc["AmmoPerClip"] = math.huge
				cc["CurrentAmmo"] = math.huge
				if Tool.Name ~= "Remington 870" then
					cc["Bullets"] = 1
					cc["AutoFire"] = true
				end
				table.insert(ReloadingGuns, Tool)
				plr.Character:FindFirstChildOfClass("Humanoid"):UnequipTools()
				Tool.Parent = plr.Character
			else
				API.Notif("You need to hold the tool you want to mod!")
			end
		end)
	else
		Temp.RemovedCommands = Temp.RemovedCommands +5
	end
