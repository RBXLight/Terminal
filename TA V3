	Script.AddCmd({"invisible","invis"},"No one can see you!",nil,function(Args)
		Toggles.InvisOn = true
		local HRP = game:GetService("Players").LocalPlayer.Character:WaitForChild("HumanoidRootPart")
		local OLDCF = HRP.CFrame
		HRP.CFrame = CFrame.new(880.9537963867188, 27.789981842041016, 2348.283447265625)
		wait(.3)
		local new = HRP:Clone()
		new.Parent = HRP.Parent
		HRP.Parent =workspace.Terrain
		new.CFrame = OLDCF
		for i,v in pairs(plr.Character:GetDescendants()) do
			pcall(function()
				if v.Name:lower() ~="humanoidrootpart" then
					v.Transparency = .7
				end
			end)
		end
	end)
