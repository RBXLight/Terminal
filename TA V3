	Script.AddCmd({'guardtools'},'Gives you guard tools being an inmate. Will give you guard clothes.', "", function()
		if API.GuardsFull() then
			API.Notif('Guard team is full! Try again later.', 'Guard Tools', 5)
			return
		end
		local Saved = plr.Character:GetPivot()
		local AutoRespawn = Toggles.AutoRespawn
		Toggles.AutoRespawn = false
		API.SwitchTeam(game.Teams.Guards)
		task.wait(1)
		local Warnings = plr.Status.innocentKills.Value
		local Targets = {}
		for _,Target in next,Players:GetPlayers() do
			if not API.Hostile(Target) then
				table.insert(Targets, Target)
			end
		end
		API.Notif(#Targets, 'Target amount', 5)
		plr.Character.Humanoid:SetStateEnabled('Dead', false)
		API.MoveTo(CFrame.new(467, 13, 1106))
		wait(1)
		for i = 1,4-plr.Status.innocentKills.Value do
			API.Kill_Player(Targets[i], {DONT_SWITCH=true,DontChangeTeam=true})
		end
		wait(2)
		API.MoveTo(Saved)
		Toggles.AutoRespawn = AutoRespawn
	end)
