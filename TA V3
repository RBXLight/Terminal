	Script.AddCmd({"fling"},"Brings a player to the void","<PLAYER>",function(Args)
		local Target = API.FindPlayer(tostring(Args[2]))
		if Target then
			local DoneFling = false
			local Switch = 1
			local cdFling = false
			task.spawn(function()
				while true do
					if game:GetService("Players").LocalPlayer.Character.HumanoidRootPart and game:GetService("Players").LocalPlayer.Character.Humanoid and game:GetService("Players").LocalPlayer.Character.Humanoid.Health > 0 then
						local Velocity = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Velocity
						game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Velocity = Velocity * 10000 + Vector3.new(10000, 10000, 10000)
						game:GetService("RunService").RenderStepped:Wait()
						game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Velocity = Velocity
						game:GetService("RunService").Stepped:Wait()
						game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Velocity = Velocity + Vector3.new(0, 0.1, 0)
					end
					if not cdFling then
						cdFling = true
						if Switch == 1 then
							plr.Character:PivotTo(Target.Character:GetPivot())
							Switch = 2
						elseif Switch == 2 then
							plr.Character:PivotTo(Target.Character:GetPivot()*CFrame.new(0,0,-4))
							Switch = 3
						else
							plr.Character:PivotTo(Target.Character:GetPivot()*CFrame.new(0,0,4))
							Switch = 1
						end
						task.spawn(function()
							wait(.06)
							cdFling = false
						end)
					end
					if Target.Character:FindFirstChild("HumanoidRootPart") and Target.Character:FindFirstChild("HumanoidRootPart").Velocity.X>50 or Target.Character:FindFirstChild("HumanoidRootPart").Velocity.Z>50 or Target.Character:FindFirstChild("HumanoidRootPart").Velocity.Y>50 or Target.Character:FindFirstChild("Humanoid").Health<0 and Target.Character:FindFirstChild("Humanoid").Sit == false then 
						break
					end
					task.wait()
				end
			end)
		end
	end)
