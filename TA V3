	Script.AddCmd({"touchfling"},"Anyone you touch gets flung","<ON/OFF>",function(Args)
		API.ToggleItem('TouchFling',Args[2])
		if Toggles.TouchFling then
			TFConnection = task.spawn(function()
				while task.wait() do
					local hrp = plr.Character.HumanoidRootPart
					local velocity = hrp.Velocity

					hrp.Velocity = velocity * 10000 + Vector3.new(10000,10000,10000)
					game:GetService('RunService').RenderStepped:Wait()
					hrp.Velocity = velocity
					game:GetService('RunService').Stepped:Wait()
					hrp.Velocity = velocity + Vector3.new(0, 0.1, 0)
				end
			end)
		else
			task.cancel(TFConnection)
		end
	end)
	Script.AddCmd({"forcefield","ff"},"Forcefield","<ON/OFF>",function(Args)
		API.ToggleItem("ff",Args[2])
		if Toggles.ff then
			Toggles.ff =  false
			wait()
			Toggles.ff = true
			while wait(.2) do
				if not plr.Character:FindFirstChildOfClass("ForceField") then
					API.SwitchTeam(game.Players.LocalPlayer.Team, true,nil,true)
				end
				if not Toggles.ff and Settings.Unloaded then
					break
				end
			end
		end
	end)
	Script.AddCmd({"serverlag"},"Lags the server until you off","<ON/OFF>",function(Args)
		API.ToggleItem("ServerLag",Args[2])
		if Toggles.ServerLag then
			API.Lag()
			local cd = false
			while wait() do
				if not Toggles.ServerLag then
					break
				end
				if Toggles.ServerLag and not cd then
					cd = true
					API.Lag()
					task.spawn(function()
						wait(10)
						cd = false
					end)
				end
			end
		end
	end)
