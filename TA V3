	Script.AddCmd({"killaura"},"Kills Players around you","<ON/OFF>, <PLAYER>",function(Args)
		if Args[2] and Args[2]:lower() ~= "off" and Args[2]:lower() ~= "on" then
			local Target = API.FindPlayer(Args[2])
			if Target then
				if not table.find(Temp.AuraPlayers,Target.Name) then
					table.insert(Temp.AuraPlayers,Target.Name)
					wait(.1)
					while wait() do
						if not table.find(Temp.AuraPlayers,Target.Name) or Settings.Unloaded then
							print("STOP")
							break
						end 
						for i,v in pairs(game:GetService("Players"):GetPlayers()) do
							pcall(function()
								if v ~=plr and v ~= Target and v.Character and v.Character:FindFirstChildOfClass("Humanoid").Health >1 and not v.Character:FindFirstChildOfClass("ForceField") and (v.Character.HumanoidRootPart.Position - Target.Character.HumanoidRootPart.Position).magnitude <20 then
									if not Temp.GobalCooldownKA then
										Temp.GobalCooldownKA = true
										API.Kill_Player(v)
										wait(.5)
										Temp.GobalCooldownKA = nil
									end
								end
							end)
						end
					end
				end
			end
		else
			API.ToggleItem("KillAura",Args[2])
		end
	end)
	Script.AddCmd({"autoitems","autoguns"},"Gives you all guns when you respawn","<ON/OFF>",function(Args)
		API.ToggleItem("AutoItems",Args[2])
		if Toggles.AutoItems then
			if game:GetService("MarketplaceService"):UserOwnsGamePassAsync(plr.UserId, 96651) or game:GetService("MarketplaceService"):UserOwnsGamePassAsync(plr.UserId, 643697197) then
				API.GetGun("M4A1")
			end
			API.GetGun("AK-47")
			API.GetGun("Remington 870")
			API.GetGun("M9")
		end
	end)
