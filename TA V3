	Script.AddCmd({"looparrest"},"Loop arrests player when criminal or more.","<PLAYER,ALL,TEAM>",function(Args)
		if Args[2] == "criminals" then
			while wait(1) do
				for i,v in pairs(game:GetService("Players"):GetPlayers()) do
					if v and v~=plr and not table.find(Temp.Whitelisted,v.Name) and v.Team == game.Teams.Criminals and not Settings.Unloaded then
						repeat RStep:Wait()
							task.spawn(function()
								API.MoveTo(v.Character.Head.CFrame)
								Remotes.ArrestEvent(v)
							end)
						until v and v.Character and v.Character["Head"]:FindFirstChildOfClass("BillboardGui") or v.Character:FindFirstChildOfClass("Humanoid").Health <1
					end
				end
				if not Temp.LoopArrestingEnables.All or Settings.Unloaded then
					break
				end
			end
		elseif Args[2] == "inmates" then
			while wait(1) do
				for i,v in pairs(game:GetService("Players"):GetPlayers()) do
					if v and v~=plr and not table.find(Temp.Whitelisted,v.Name) and v.Team == game.Teams.Inmates and API.IsBadArea(v) and not Settings.Unloaded then
						repeat RStep:Wait()
							task.spawn(function()
								API.MoveTo(v.Character.Head.CFrame)
								Remotes.ArrestEvent(v)
							end)
						until v and v.Character and v.Character["Head"]:FindFirstChildOfClass("BillboardGui") or v.Character:FindFirstChildOfClass("Humanoid").Health <1
					end
				end
				if not Temp.LoopArrestingEnables.All or Settings.Unloaded then
					break
				end
			end
		elseif Args[3] == "all" then
			while wait(1) do
				for i,v in pairs(game:GetService("Players"):GetPlayers()) do
					if v and v~=plr and not table.find(Temp.Whitelisted,v.Name) and v.Team == game.Teams.Criminals or API.IsBadArea(v) and v ~=plr and not Settings.Unloaded then
						repeat RStep:Wait()
							task.spawn(function()
								API.MoveTo(v.Character.Head.CFrame)
								Remotes.ArrestEvent(v)
							end)
						until v and v.Character and v.Character["Head"]:FindFirstChildOfClass("BillboardGui") or v.Character:FindFirstChildOfClass("Humanoid").Health <1
					end
				end
				if not Temp.LoopArrestingEnables.All or Settings.Unloaded then
					break
				end
			end
		else
			local Target = API.FindPlayer(tostring(Args[2]))
			if Target then
				if not table.find(Temp.LoopArresting,Target.Name) then
					return
				else
					table.insert(Temp.LoopArresting,Target.Name)
				end
				while wait() do
					if Target.Team == game.Teams.Criminals or API.IsBadArea(Target) then
						repeat RStep:Wait()
							task.spawn(function()
								API.MoveTo(Target.Character.Head.CFrame)
								Remotes.ArrestEvent(Target)
							end)
						until Target.Character["Head"]:FindFirstChildOfClass("BillboardGui") or Target.Character:FindFirstChildOfClass("Humanoid").Health <1
					end
					if not table.find(Temp.LoopArresting,Target.Name) then
						break
					end
				end
			end
		end
	end)
	Script.AddCmd({"unlooparrest"},"Stops loop arresting","<PLAYER,ALL,TEAM>",function(Args)
