	Script.AddCmd({"antipunch"},"Kills people who punch you","<ON/OFF>",function(Args)
		API.ToggleItem("AntiPunch",Args[2])
		if Toggles.AntiPunch then
			Temp.ConnectionsPUNCH = {}
			for i,v in pairs(game:GetService("Players"):GetPlayers()) do
				if v and v~=plr then
					task.spawn(function()
						API.AntiPunchC(v)
					end)
					Temp.ConnectionsPUNCH[i] = v.CharacterAdded:Connect(function(c)
						repeat task.wait() until c and c:FindFirstChildOfClass("Humanoid")
						if Toggles.AntiPunch then
							if Toggles.AntiPunch then
								API.AntiPunchC(v)
							end
						end
					end)
				end
			end
		else
			for i,v in pairs(Temp.ConnectionsPUNCH) do
				if v then
					v:Disconnect()
				end
			end
			Temp.ConnectionsPUNCH = nil
		end
	end)
	Script.AddCmd({'breakserver'}, "Breaks shields,Doors and carspawners", "<ON/OFF>", function(Args)
		API.ToggleItem("BreakServer",Args[2])
		if Toggles.BreakServer then
			while wait(3) do
				if not Toggles.BreakServer or Settings.Unloaded then break end
				for i,v in pairs(workspace.Prison_ITEMS.buttons:GetDescendants()) do
					if v and v.Parent.Name:lower():find("car ") and v.Name:lower()=="deb" then
						API.ToggleValue(v)
					end
				end
				for i,v in pairs(game.Teams.Guards:GetPlayers()) do
					if v then
						local Shield = v.Backpack:FindFirstChild("Riot Shield") or v.Character:FindFirstChild("Riot Shield")
						if Shield then
							API.ToggleValue(Shield.isEquipped)
						end
					end
				end
				for i,v in pairs(workspace.Doors:GetChildren()) do
					API.ToggleValue(v.isActive) 
				end
			end
		end
	end)
	Script.AddCmd({'breakcarspawner'}, "Breaks car spawners", "<ON/OFF>", function(Args)
