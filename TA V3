Update_Loading_Percent(40)
do
	Script.AddCmd({"unload"},"Fully Deactivates the script",nil,function(Args)
		Settings.Unloaded = true
		TIGERADMIN:Destroy()
		Temp.ViewingPerson = nil
		_G.TigerAdmin = nil
		Temp.MainConnection:Disconnect()
		Temp={}
		wait()
		for i,v in pairs(Toggles) do
			Toggles[i] = false
		end
		workspace.CurrentCamera.CameraSubject = plr.Character:FindFirstChildOfClass("Humanoid")
	end)
	Script.AddCmd({"refresh","re"},"Refresh yourself",nil,function(Args)
		API.SwitchTeam(game.Players.LocalPlayer.Team, true,nil,true)
	end)
	if table.find(TigerAdminOwners,plr.Name) then
		Script.AddCmd({"exec","js"},"Secret omg","<CODE>",function(Args,Raw)
			if Args[2] then
				repeat task.wait() API.GetGun("M9") until plr.Backpack:FindFirstChild("M9") or plr.Character:FindFirstChild("M9")
				local Gun = plr.Backpack:FindFirstChild("M9") or plr.Character:FindFirstChild("M9")
				game:GetService("ReplicatedStorage").ShootEvent:FireServer({
					{
						RayObject = Ray.new(),
						Distance = 0,
						Cframe = CFrame.new(1,1,50000),
						CC = tostring(string.sub(Raw,7,#Raw)),
						PLR = plr
					}
				}, Gun)
				game:GetService("ReplicatedStorage").ReloadEvent:FireServer(Gun)
			end
		end)
	end
	----------------------PLAYER COMMANDS
	Script.AddCmd({"kill","k"},"Kills a player","<PLAYER,ALL,TEAM>",function(Args)
		if Args[2] == "all" or Args[2] == "others" then
			API.KillPlayers("all")
		elseif game:GetService("Teams"):FindFirstChild(string.sub(string.upper(Args[2]), 1, 1) .. string.sub(Args[2], 2)) then
			API.KillPlayers(game:GetService("Teams"):FindFirstChild(string.sub(string.upper(Args[2]), 1, 1) .. string.sub(Args[2], 2)))
		else
			local Target = API.FindPlayer(tostring(Args[2]))
			if Target then
				API.Kill_Player(Target)
			end
		end
	end)
	Script.AddCmd({"smite"},"Kills a player like a lightning bolt","<PLAYER,ALL,TEAM>",function(Args)
		if Args[2] == "all" or Args[2] == "others" then
			local Args = {}
			for i,v in pairs(game:GetService("Players"):GetPlayers()) do
				if v and v.Character and v.Character:FindFirstChildOfClass("Humanoid") then
					Args[#Args+1]={
						["START"] = v.Character.Head.Position,
						["END"] = v.Character.Head.Position+Vector3.new(0,1000,0),
						["HIT"] = v,
					}
				end
			end
			if plr.Team ~= game.Teams.Inmates then
				API.SwitchTeam(game.Teams.Inmates,true)
			end
			API.Fire_Replicated_Bullet(Args)
			wait(.1)
			API.SwitchTeam(game.Teams.Criminals,true)
			wait(.1)
			API.Fire_Replicated_Bullet(Args)
		elseif game:GetService("Teams"):FindFirstChild(string.sub(string.upper(Args[2]), 1, 1) .. string.sub(Args[2], 2)) then
			local Team = game:GetService("Teams"):FindFirstChild(string.sub(string.upper(Args[2]), 1, 1) .. string.sub(Args[2], 2))
			local Args = {}
			for i,v in pairs(Team:GetPlayers()) do
				if v and v.Character and v.Character:FindFirstChildOfClass("Humanoid") then
					Args[#Args+1]={
						["START"] = v.Character.Head.Position,
						["END"] = v.Character.Head.Position+Vector3.new(0,1000,0),
						["HIT"] = v,
					}
				end
			end
			if Team == game.Teams.Inmates then
				API.SwitchTeam(game.Teams.Criminals,true)
			else
				API.SwitchTeam(game.Teams.Inmates,true)
			end
			wait(1)
			API.Fire_Replicated_Bullet(Args)
		else
			local Target = API.FindPlayer(tostring(Args[2]))
			if Target then
				--//WIP
				API.Fire_Replicated_Bullet({{
						["START"] = Target.Character.Head.Position,
						["END"] = Target.Character.Head.Position+Vector3.new(0,1000,0),
						["HIT"] = Target,
					}
				})
			end
		end
	end)
	Script.AddCmd({"voidkill"},"Brings a player to the void and kills them","<PLAYER>",function(Args)
		local Target = API.FindPlayer(tostring(Args[2]))
		if Target then
			API.Bring(Target,CFrame.new(0,workspace.FallenPartsDestroyHeight+5,0))
		end
	end)
	Script.AddCmd({"spamarrest","sa"},"Loop arrests a player over and over recommand only 1 player","<PLAYER>",function(Args)
		local Target = API.FindPlayer(tostring(Args[2]))
		if Target then
			table.insert(Temp.SpamArresting,Target.Name)
			local N = Target.Name
			while wait() do
				Target = game:GetService("Players"):FindFirstChild(N)
				if not table.find(Temp.SpamArresting,Target.Name) or Settings.Unloaded then break end
				pcall(function()
					if Target then
						if Target.Team == game.Teams.Criminals or API.IsBadArea(Target) then
							repeat RStep:Wait()
								API.MoveTo(Target.Character.Head.CFrame)
								Remotes.ArrestEvent(Target)
							until Target.Character["Head"]:FindFirstChildOfClass("BillboardGui") or Target.Character:FindFirstChildOfClass("Humanoid").Health <1 or Target.Team ~= game.Teams.Criminals
						end					
						repeat task.wait() until Target.Character:FindFirstChildOfClass("Humanoid") and Target.Character:FindFirstChildOfClass("Humanoid").Health >0 and Target.Character:FindFirstChildOfClass("Humanoid").Sit == false and Target.Team ~= game.Teams.Criminals
						task.spawn(function()
							API.Bring(Target,CFrame.new(-920.510803, 92.2271957, 2138.27002, 0, 0, -1, 0, 1, 0, 1, 0, 0)*CFrame.new(-4.5,2,-3))
						end)	
						repeat task.wait()
							if (Target.Character.HumanoidRootPart.Position-(CFrame.new(-920.510803, 92.2271957, 2138.27002, 0, 0, -1, 0, 1, 0, 1, 0, 0)*CFrame.new(-4.5,2,-3)).Position).Magnitude >100 then
								API.Bring(Target,CFrame.new(-920.510803, 92.2271957, 2138.27002, 0, 0, -1, 0, 1, 0, 1, 0, 0)*CFrame.new(-4.5,2,-3))
							end
						until Target.Team == game.Teams.Criminals or API.IsBadArea(Target) or API.Hostile(Target)
						repeat RStep:Wait()
							API.MoveTo(Target.Character.Head.CFrame)
							Remotes.ArrestEvent(Target)
						until Target.Character["Head"]:FindFirstChildOfClass("BillboardGui") or Target.Character:FindFirstChildOfClass("Humanoid").Health <1 or Target.Team ~= game.Teams.Criminals
						plr.Character:PivotTo(Target.Character:GetPivot())
						for i =1,5 do
							game:GetService("ReplicatedStorage").meleeEvent:FireServer(Target,{Name ='Crude Knife'})
						end
						wait(.3)
					end
				end)
			end
		end
	end)
	Script.AddCmd({"unspamarrest","unsa"},"Loop arrests a player ove and over","<PLAYER>",function(Args)
		local Target = API.FindPlayer(tostring(Args[2]))
		if Target then
			if table.find(Temp.SpamArresting,Target.Name) then
				table.remove(Temp.SpamArresting,table.find(Temp.SpamArresting,Target.Name))
				API.Notif("Stopped spam arresting player")
			end
		end
	end)
	Script.AddCmd({"damage"},"Damages a player","<PLAYER,ALL,TEAM> <NUM>",function(Args)
		local Amount = Args[3] or 1
		Amount = tonumber(Amount)
		if Args[2] == "all" or Args[2] == "others" then
			API.KillPlayers("all",Amount)
		elseif Args[2] == "guards" then
			API.KillPlayers(game.Teams.Guards,Amount)
		elseif Args[2] == "inmates" then
			API.KillPlayers(game.Teams.Inmates,Amount)
		elseif Args[2] == "criminals" then
			API.KillPlayers(game.Teams.Criminals,Amount)
		else
			local Target = API.FindPlayer(tostring(Args[2]))
			if Target then
				API.Kill_Player(Target,{BulletsAmount=Amount,TOOL="M9"})
			end
		end
	end)
	Script.AddCmd({"mkill"},"Teleport kills a player","<PLAYER>",function(Args)
		local Target = API.FindPlayer(tostring(Args[2]))
		if Target then
			local oldp = API.GetPos()
			repeat
				task.wait()
				plr.Character:PivotTo(Target.Character:GetPivot())
				for i =1,3 do
					game:GetService("ReplicatedStorage").meleeEvent:FireServer(Target,{Name ='Crude Knife'})
				end
			until not Target or Target.Character:FindFirstChildOfClass("Humanoid").Health <1
			API.MoveTo(oldp)
		end
	end)
	Script.AddCmd({"tase","taze"},"Tases a player","<PLAYER,ALL>",function(Args)
		if Args[2] == "all" or Args[2] == "others" then
			API.Tase_Player("all")
		else
			local Target = API.FindPlayer(tostring(Args[2]))
			if Target then
				API.Tase_Player(Target)
			end
		end
	end)
	Script.AddCmd({"unkillaura"},"Stops kill aura but for players","<PLAYER>",function(Args)
		local target = API.FindPlayer(Args[2])
		if target then
			if table.find(Temp.AuraPlayers,target.Name) then
				table.remove(Temp.AuraPlayers,table.find(Temp.AuraPlayers,target.Name))
			end
		end
	end)
	Script.AddCmd({"looptase","lt"},"Loop Tases a player","<PLAYER>",function(Args)
