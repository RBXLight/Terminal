	DOWN.BackgroundColor3 = Color3.fromRGB(47, 47, 47)
	DOWN.BackgroundTransparency = 0.150
	DOWN.BorderColor3 = Color3.fromRGB(0, 0, 0)
	DOWN.BorderSizePixel = 0
	DOWN.Position = UDim2.new(0.24622868, 0, 0.573934853, 0)
	DOWN.Size = UDim2.new(0.105473474, 0, 0.134085208, 0)
	DOWN.Font = Enum.Font.SourceSans
	DOWN.Text = "DWN"
	DOWN.TextColor3 = Color3.fromRGB(255, 255, 255)
	DOWN.TextScaled = true
	DOWN.TextSize = 14.000
	DOWN.TextWrapped = true

	UIStroke___5.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
	UIStroke___5.Parent = DOWN
	UIStroke___5.Name = "UIStroke__"

	UICorner___5.Name = "UICorner__"
	UICorner___5.Parent = DOWN

	UITextSizeConstraint___5.Name = "UITextSizeConstraint__"
	UITextSizeConstraint___5.Parent = DOWN
	UITextSizeConstraint___5.MaxTextSize = 71

	UIAspectRatioConstraint_5.Parent = DOWN
	UIAspectRatioConstraint_5.AspectRatio = 0.997

	UP.Name = "UP"
	UP.Parent = TIGERADMIN
	UP.BackgroundColor3 = Color3.fromRGB(47, 47, 47)
	UP.BackgroundTransparency = 0.150
	UP.BorderColor3 = Color3.fromRGB(0, 0, 0)
	UP.BorderSizePixel = 0
	UP.Position = UDim2.new(0.00686171604, 0, 0.573934853, 0)
	UP.Size = UDim2.new(0.105473474, 0, 0.134085208, 0)
	UP.Font = Enum.Font.SourceSans
	UP.Text = "UP"
	UP.TextColor3 = Color3.fromRGB(255, 255, 255)
	UP.TextScaled = true
	UP.TextSize = 14.000
	UP.TextWrapped = true

	UIStroke___6.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
	UIStroke___6.Parent = UP
	UIStroke___6.Name = "UIStroke__"

	UICorner___6.Name = "UICorner__"
	UICorner___6.Parent = UP

	UITextSizeConstraint___6.Name = "UITextSizeConstraint__"
	UITextSizeConstraint___6.Parent = UP
	UITextSizeConstraint___6.MaxTextSize = 71

	UIAspectRatioConstraint_6.Parent = UP
	UIAspectRatioConstraint_6.AspectRatio = 0.997

	local WASD_PHONEKEYS ={
		w = false,
		a = false,
		s = false,
		d = false,
	}
	A.MouseEnter:Connect(function()
		WASD_PHONEKEYS.a = true
	end)
	A.MouseLeave:Connect(function()
		WASD_PHONEKEYS.a = false
	end)
	S.MouseEnter:Connect(function()
		WASD_PHONEKEYS.s = true
	end)
	S.MouseLeave:Connect(function()
		WASD_PHONEKEYS.s = false
	end)
	D.MouseEnter:Connect(function()
		WASD_PHONEKEYS.d = true
	end)
	D.MouseLeave:Connect(function()
		WASD_PHONEKEYS.d = false
	end)
	W.MouseEnter:Connect(function()
		WASD_PHONEKEYS.w = true
	end)
	W.MouseLeave:Connect(function()
		WASD_PHONEKEYS.w = false
	end)
	local KEY = Enum.KeyCode.E
	UP.MouseEnter:Connect(function()
		WASD_PHONEKEYS.e = true
	end)
	UP.MouseLeave:Connect(function()
		WASD_PHONEKEYS.e = false
	end)
	DOWN.MouseEnter:Connect(function()
		WASD_PHONEKEYS.q = true
	end)
	DOWN.MouseLeave:Connect(function()
		WASD_PHONEKEYS.q = false
	end)
	Flying = true
	local Controler = (HasRequire and require(game:GetService("Players").LocalPlayer.PlayerScripts:WaitForChild('PlayerModule'):WaitForChild("ControlModule")))
	if not Controler then Controler = nil end
	if game:GetService("UserInputService").TouchEnabled and game:GetService("UserInputService").KeyboardEnabled == false then
		if Controler or not HasRequire then
			W.Visible = true
			A.Visible = true
			S.Visible = true
			D.Visible = true
			UP.Visible = true
			DOWN.Visible = true
			Temp.FLYKEY_W = W
			Temp.FLYKEY_A = A
			Temp.FLYKEY_S = S
			Temp.FLYKEY_D = D
			Temp.FLYKEY_UP = UP
			Temp.FLYKEY_DOWN = DOWN
		end
	else
		W.Visible = false
		A.Visible = false
		S.Visible = false
		D.Visible = false
		UP.Visible = false
		DOWN.Visible = false
	end
	local BodyA,BodyB = Instance.new("BodyVelocity"),Instance.new("BodyGyro")
	BodyA.Parent = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart
	BodyA.MaxForce = Vector3.new(0,0,0)
	BodyA.Velocity = Vector3.new(0,0,0)
	BodyB.Parent = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart
	BodyB.MaxTorque = Vector3.new(4^234,4^234,4^234)
	BodyB.P = 10000
	BodyB.D = 50
	Temp.FlyA = BodyA
	Temp.FlyB = BodyB
	local camera = game.Workspace.CurrentCamera
	Temp.FlyConnection = game:GetService"RunService".RenderStepped:Connect(function()
		if game:GetService("Players").LocalPlayer.Character and game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid").Health >0 and BodyA and BodyB then
			local direction = {x=0,z=0}
			if Controler and direction then
				direction=(Controler:GetMoveVector())
			end
			BodyA.MaxForce = Vector3.new(4^234,4^234,4^234)
			BodyB.MaxTorque = Vector3.new(4^234,4^234,4^234)
			BodyB.CFrame = camera.CoordinateFrame
			BodyA.Velocity = Vector3.new()
			if direction and Controler then --// imlazy
				if direction.X > 0 then
					BodyA.Velocity = BodyA.Velocity + camera.CFrame.RightVector*(direction.X*(speed*10))
				end
				if direction.X < 0 then
					BodyA.Velocity = BodyA.Velocity + camera.CFrame.RightVector*(direction.X*(speed*10))
				end
				if direction.Z > 0 then
					BodyA.Velocity = BodyA.Velocity - camera.CFrame.LookVector*(direction.Z*(speed*10))
				end
				if direction.Z < 0 then
					BodyA.Velocity = BodyA.Velocity - camera.CFrame.LookVector*(direction.Z*(speed*10))
				end
			else
				if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.A) or WASD_PHONEKEYS.a then
					BodyA.Velocity = BodyA.Velocity + camera.CFrame.RightVector*((1*(speed*10))*-1)
				end
				if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.D) or WASD_PHONEKEYS.d then
					BodyA.Velocity = BodyA.Velocity + camera.CFrame.RightVector*(1*(speed*10))
				end
				if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.W) or WASD_PHONEKEYS.w then
					BodyA.Velocity = BodyA.Velocity - camera.CFrame.LookVector*((1*(speed*10))*-1)
				end
				if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.S)  or WASD_PHONEKEYS.s then
					BodyA.Velocity = BodyA.Velocity - camera.CFrame.LookVector*((1*(speed*10)))
				end
				if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.Q) or WASD_PHONEKEYS.q then
					BodyA.Velocity = BodyA.Velocity - Vector3.new(0,30,0)
				end
				if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.E) or WASD_PHONEKEYS.e then
					BodyA.Velocity = BodyA.Velocity + Vector3.new(0,30,0)
				end
			end
		end
	end)
end
function API.Unfly()
	if Flying then
		Flying = false
		Temp.FlyA:Destroy()
		Temp.FlyB:Destroy()
		Temp.FlyConnection:Disconnect()
		API.SwitchTeam(plr.Team,true)
		if Temp.FLYKEY_W~=nil  then
			Temp.FLYKEY_W:Destroy()
			Temp.FLYKEY_A:Destroy()
			Temp.FLYKEY_S:Destroy()
			Temp.FLYKEY_D:Destroy()
			Temp.FLYKEY_UP:Destroy()
			Temp.FLYKEY_DOWN:Destroy()
		end
	end
end
local Remotes = { --// i dont even use this shit a lot
	ArrestEvent = function(arg)
		if arg and arg.Character and arg.Character:FindFirstChild("HumanoidRootPart")  then
			if (API.GetPos().Position-arg.Character:FindFirstChild("HumanoidRootPart").Position).Magnitude <=14 then
				if API.IsBadArea(arg) or arg.Team == game.Teams.Criminals then
					workspace.Remote.arrest:InvokeServer(arg.Character:FindFirstChildWhichIsA("Part"))
				end
			end
		end
	end,
	EquipShield = function(arg)
		workspace.Remote.equipShield:FireServer(arg)
	end,
}

local FindTool = function(item)
	local tool = Players:FindFirstChild(item, true)
	if not tool then
		for i,v in pairs(Players:GetChildren()) do
			if v.Character and v.Character:FindFirstChild(item) then
				tool = v.Character[item]
				break
			end
		end
	end
	return tool
end
API.Tase_Player = function(Target)
	local LastTeam = plr.Team
	local Args = {}
	local Gun = nil
	if API.GuardsFull() == false then
		if Target then
			if Target == "all" then
				for i,v in pairs(game:GetService("Players"):GetPlayers()) do
					if v and v.Character and v.Character:FindFirstChildOfClass("Humanoid") and v.Character:FindFirstChildOfClass("Humanoid").Health >1 and not table.find(Temp.Whitelisted,v.Name) and v.Team ~= game.Teams.Guards then
						Args[#Args + 1] = {
							["RayObject"] = Ray.new(Vector3.new(), Vector3.new()),
							["Distance"] = 0,
							["Cframe"] = CFrame.new(),
							["Hit"] = v.Character:FindFirstChild("Head"),
						}
					end
				end
				if plr.Team ~= game.Teams.Guards then
					API.SwitchTeam(game.Teams.Guards,true)
				end
				repeat RStep:Wait() until plr.Character and plr.Character:FindFirstChildOfClass("Humanoid") and plr.Backpack:FindFirstChild("Taser")
				Gun = plr.Backpack:FindFirstChild("Taser")
				game:GetService("ReplicatedStorage"):WaitForChild("ShootEvent"):FireServer(Args, Gun)
				game:GetService("ReplicatedStorage").ReloadEvent:FireServer(Gun)
				wait()
				API.SwitchTeam(LastTeam,true)
			else
				Args[#Args + 1] = {
					["RayObject"] = Ray.new(Vector3.new(), Vector3.new()),
					["Distance"] = 0,
					["Cframe"] = CFrame.new(),
					["Hit"] = Target.Character:FindFirstChild("Head"),
				}
				if plr.Team ~= game.Teams.Guards then
					API.SwitchTeam(game.Teams.Guards,true)
				end
				repeat RStep:Wait() until plr.Character and plr.Character:FindFirstChildOfClass("Humanoid") and plr.Backpack:FindFirstChild("Taser")
				Gun = plr.Backpack:FindFirstChild("Taser")
				game:GetService("ReplicatedStorage"):WaitForChild("ShootEvent"):FireServer(Args, Gun)
				game:GetService("ReplicatedStorage").ReloadEvent:FireServer(Gun)
				wait()
				API.SwitchTeam(LastTeam,true)
			end
		end
	else
		API.Notif("Guards team is full!")
	end
end
API.KillPlayers = function(Team,BulletsAmount)
	local LastTeam = plr.Team
	local Args = {}
	local Gun = nil
	if Team == "all" then
		for i,v in pairs(game:GetService("Players"):GetPlayers()) do
			if v and v.Character and v.Character:FindFirstChildOfClass("Humanoid") and v.Character:FindFirstChildOfClass("Humanoid").Health >1 and not v.Character:FindFirstChildOfClass("ForceField") and not table.find(Temp.Whitelisted,v.Name) then
				for i =1,(BulletsAmount ~=nil and BulletsAmount or 8) do
					Args[#Args + 1] = {
						["RayObject"] = Ray.new(Vector3.new(), Vector3.new()),
						["Distance"] = 0,
						["Cframe"] = CFrame.new(),
						["Hit"] = v.Character:FindFirstChild("Head"),
					}
				end
			end
		end
		--//KILL GUARDS + CRIMINALS
		if plr.Team ~= game.Teams.Inmates then
			API.SwitchTeam(game.Teams.Inmates,true)
		end
		repeat RStep:Wait() until plr.Character and plr.Character:FindFirstChildOfClass("Humanoid")
		API.GetGun("Remington 870")
		repeat RStep:Wait() API.GetGun("Remington 870") Gun = plr.Backpack:FindFirstChild("Remington 870") or plr.Character:FindFirstChild("Remington 870")  until plr.Backpack:FindFirstChild("Remington 870") or plr.Character:FindFirstChild("Remington 870") and plr.Character:FindFirstChildOfClass("Humanoid")
		game:GetService("ReplicatedStorage"):WaitForChild("ShootEvent"):FireServer(Args, Gun)
		game:GetService("ReplicatedStorage").ReloadEvent:FireServer(Gun)
		--//KILL INMATES
		API.SwitchTeam(game.Teams.Criminals,true)
		repeat RStep:Wait() until plr.Character and plr.Character:FindFirstChildOfClass("Humanoid")
		API.GetGun("Remington 870")
		repeat RStep:Wait() API.GetGun("Remington 870") Gun = plr.Backpack:FindFirstChild("Remington 870") or plr.Character:FindFirstChild("Remington 870")  until plr.Backpack:FindFirstChild("Remington 870") or plr.Character:FindFirstChild("Remington 870") and plr.Character:FindFirstChildOfClass("Humanoid")
		game:GetService("ReplicatedStorage"):WaitForChild("ShootEvent"):FireServer(Args, Gun)
		game:GetService("ReplicatedStorage").ReloadEvent:FireServer(Gun)
		task.spawn(function()
			wait()
			API.SwitchTeam(LastTeam,true)
		end)
	else
		for i,v in pairs(Team:GetPlayers()) do
			if v and v.Character and v.Character:FindFirstChildOfClass("Humanoid") and v.Character:FindFirstChildOfClass("Humanoid").Health >1 and not v.Character:FindFirstChildOfClass("ForceField") and not table.find(Temp.Whitelisted,v.Name) then
				for i =1,6 do
					Args[#Args + 1] = {
						["RayObject"] = Ray.new(Vector3.new(), Vector3.new()),
						["Distance"] = 0,
						["Cframe"] = CFrame.new(),
						["Hit"] = v.Character:FindFirstChild("Head"),
					}
				end
			end
		end
		if Team == plr.Team then
			if Team~= game.Teams.Criminals then
				API.SwitchTeam(game.Teams.Criminals,true)
			elseif Team == game.Teams.Criminals then
				API.SwitchTeam(game.Teams.Inmates,true)
			end
		end
		task.wait()
		API.GetGun("Remington 870")
		repeat RStep:Wait() API.GetGun("Remington 870") Gun = plr.Backpack:FindFirstChild("Remington 870") or plr.Character:FindFirstChild("Remington 870")  until plr.Backpack:FindFirstChild("Remington 870") or plr.Character:FindFirstChild("Remington 870") and plr.Character:FindFirstChildOfClass("Humanoid")
		game:GetService("ReplicatedStorage"):WaitForChild("ShootEvent"):FireServer(Args, Gun)
		game:GetService("ReplicatedStorage").ReloadEvent:FireServer(Gun)
		task.wait()
		API.SwitchTeam(LastTeam,true)
	end
end

API.SwitchTeam = function(TeamPath,DeleteForceField,AutoRespawn,IsRefresh,RespawnAt)
	local LastPos,LastCamRotation,CrimSpawn,LastPosCrim = plr.Character:FindFirstChild("HumanoidRootPart").CFrame,workspace.CurrentCamera.CFrame,game:GetService("Workspace")["Criminals Spawn"]:GetChildren()[1],nil
	if RespawnAt then LastPos = RespawnAt end
	local Connection = nil
	LastPosCrim = CrimSpawn.CFrame
	Connection = plr.CharacterAdded:Connect(function(char)
		coroutine.wrap(function()
			workspace.CurrentCamera:GetPropertyChangedSignal("CFrame"):Wait()
			workspace["CurrentCamera"].CFrame = LastCamRotation
			workspace["CurrentCamera"].CFrame = LastCamRotation
			workspace["CurrentCamera"].CFrame = LastCamRotation
			local hrp = char:WaitForChild("HumanoidRootPart")
			if hrp then
				hrp.CFrame = (RespawnAt or LastPos)
			end
		end)()
		char:WaitForChild("HumanoidRootPart")
		if DeleteForceField then
			task.spawn(function()
				repeat task.wait() until plr.Character:FindFirstChildOfClass("ForceField")
				plr.Character:FindFirstChildOfClass("ForceField").Visible = false
			end)
		end
		if AutoRespawn then
			pcall(function()
				plr.Character:FindFirstChildOfClass("Humanoid").BreakJointsOnDeath = false
			end)
		end
		task.wait()
		API.MoveTo(LastPos,nil)
		task.spawn(function()
			wait(.75)
			if (LastPos.Position-API.GetPos().Position).Magnitude >30 then
				API.MoveTo(LastPos,nil)
			end
		end)
		if Connection then
			Connection:Disconnect()
		else
			API.SwitchTeam(TeamPath,DeleteForceField,AutoRespawn,IsRefresh,RespawnAt)
		end
		if IsRefresh and TeamPath == game.Teams.Criminals  then
			repeat
				CrimSpawn.Transparency = 1
				CrimSpawn.CanCollide = false
				CrimSpawn.CFrame = API.GetPos()
				RStep:Wait()
			until plr.Team == game:GetService("Teams").Criminals
			CrimSpawn.CFrame = LastPosCrim
			CrimSpawn.Transparency = 0
		end
	end)
	if TeamPath == game.Teams.Criminals then
		if IsRefresh then
			workspace['Remote']['TeamEvent']:FireServer("Bright orange")
		else
			repeat
				CrimSpawn.Transparency = 1
				CrimSpawn.CanCollide = false
				CrimSpawn.CFrame = API.GetPos()
				RStep:Wait()
			until plr.Team == game:GetService("Teams").Criminals
			CrimSpawn.CFrame = LastPosCrim
			CrimSpawn.Transparency = 0
			Connection:Disconnect()
		end
	else
		if TeamPath == game.Teams.Neutral then
			workspace['Remote']['TeamEvent']:FireServer("Bright orange")
		else
			if not TeamPath or not TeamPath.TeamColor then
				workspace['Remote']['TeamEvent']:FireServer("Bright orange")
			else
				workspace['Remote']['TeamEvent']:FireServer(TeamPath.TeamColor.Name)
			end
		end
	end
end
API.Fire_Replicated_Bullet = function(Args,DoDamage)
	local Bullets = {}
	for i,v in pairs(Args) do
		local dist = (v.START-v.END).magnitude
		local s = (dist/1)
		local vector = Vector3.new(math.random(-s,s)/10,math.random(-s,s)/10,math.random(-s,s)/10)
		local Ray = Ray.new(v.START, ((v.END + vector)- v.START).unit*100000)
		local position =  v.END
		local distance = (position - v.START).magnitude
		local rayPart = Instance.new("Part",workspace)
		local mesh = Instance.new("BlockMesh",rayPart)
		mesh.Scale = Vector3.new(.5,.5,1)
		rayPart.Name = "RayPart"
		rayPart.BrickColor = BrickColor.Yellow()
		rayPart.Material = Enum.Material.Neon
		rayPart.Anchored = true
		rayPart.CanCollide = false
		rayPart.Transparency = .5
		rayPart.formFactor = Enum.FormFactor.Custom
		rayPart.Size = Vector3.new(.2,.2,distance)
		rayPart.CFrame = CFrame.new(position,v.START) * CFrame.new(0,0,-distance/2)
		game:GetService("Debris"):AddItem(rayPart,.05)
		local RealHit,RealPos = workspace:FindPartOnRay(Ray,plr.Character)
		for i =1,((v.HIT and v.HIT:IsA("Player") and 9) or 1) do
			Bullets[#Bullets + 1] = {
				["RayObject"] = Ray,
				["Distance"] = distance,
				["Cframe"] = rayPart.CFrame,
				["Hit"] = (v.HIT and v.HIT:IsA("Player") and v.HIT.Character.Head) or (DoDamage and RealHit) or workspace:FindFirstChildOfClass("Part"),
			}
		end
	end
	local Tool,Gun = (Settings.TOOL or "Remington 870"),nil
	repeat task.wait() API.GetGun(Tool) Gun = plr.Backpack:FindFirstChild(Tool) or plr.Character:FindFirstChild(Tool) until plr.Backpack:FindFirstChild(Tool) or plr.Character:FindFirstChild(Tool) and plr.Character:FindFirstChildOfClass("Humanoid")
	game:GetService("ReplicatedStorage"):WaitForChild("ShootEvent"):FireServer(Bullets, Gun)
	game:GetService("ReplicatedStorage").ReloadEvent:FireServer(Gun)
end
API.Kill_Player = function(Target,SETTINGS)
	if table.find(Temp.Whitelisted,Target.Name) or Settings.Unloaded then
		return
	end
	if (not SETTINGS) then SETTINGS = {} end
	if Target and Target.Character:FindFirstChildOfClass("Humanoid") and Target.Character:FindFirstChildOfClass("Humanoid").Health >0 then
		if not Settings.DISABLE_CLOSE and not SETTINGS.BulletsAmount then
			if (Target.Character:GetPivot().Position-plr.Character:GetPivot().Position).Magnitude <15 then
				for i = 1,8 do
					game:GetService("ReplicatedStorage").meleeEvent:FireServer(Target,{Name ='Crude Knife'})
				end
				return
			end
		end
		local Gun,Args,CurrentTeam = nil,{},plr.Team
		for i =1,(SETTINGS.BulletsAmount ~=nil and SETTINGS.BulletsAmount or 9) do
			Args[#Args + 1] = {
				["RayObject"] = Ray.new(Vector3.new(), Vector3.new()),
				["Distance"] = 0,
				["Cframe"] = CFrame.new(),
				["Hit"] = Target.Character:FindFirstChild("Head"),
			}
		end
		if not SETTINGS.DONT_SWITCH then
			if Target.Team == plr.Team then
				if Target.Team~= game.Teams.Criminals then
					API.SwitchTeam(game.Teams.Criminals,true)
				elseif Target.Team == game.Teams.Criminals then
					API.SwitchTeam(game.Teams.Inmates,true)
				elseif plr.Team == game.Teams.Guards and not API.Hostile(Target) then
					API.SwitchTeam(game.Teams.Inmates,true)
				end
			end
		end
		task.wait()
		repeat task.wait() until plr.Character and plr.Character:FindFirstChildOfClass("Humanoid")
		local Tool = (Settings.TOOL or "Remington 870")
		repeat task.wait() API.GetGun(Tool) Gun = plr.Backpack:FindFirstChild(Tool) or plr.Character:FindFirstChild(Tool) until plr.Backpack:FindFirstChild(Tool) or plr.Character:FindFirstChild(Tool) and plr.Character:FindFirstChildOfClass("Humanoid")
		game:GetService("ReplicatedStorage"):WaitForChild("ShootEvent"):FireServer(Args, Gun)
		game:GetService("ReplicatedStorage").ReloadEvent:FireServer(Gun)
		task.wait()
		if not SETTINGS.DontChangeTeam then
			API.SwitchTeam(CurrentTeam,true)
		end
	end
end
API.ToggleItem = function(Item,v)
	if Toggles[Item] == nil then
		Toggles[Item] = false
	end
	if Item and Toggles[Item]~=nil then
		if not v then
			Toggles[Item]=not Toggles[Item]
		elseif typeof(v):lower() == "string" and v:lower() == "on" then
			Toggles[Item] = true
		elseif typeof(v):lower() == "string" and v:lower() == "off" then
			Toggles[Item] = false
		end
		API.Notif(Item.." was switched to "..tostring(Toggles[Item]))
	end
end
API.Hostile = function(Player) 
	local Tools = {'M4A1', 'Remington 870', 'AK-47', 'M9', 'Hammer', 'Crude Knife'}
	local Hostile
	if Player:FindFirstChild('Backpack') then
		for _,Tool in next,Player.Backpack:children() do
			if table.find(Tools, Tool.Name) then
				Hostile = true
				break
			end
		end
	end
	if not Hostile and Player.Character then
		for _,Tool in next,Player.Character:children() do
			if Tool:IsA('Tool') and table.find(Tools, Tool.Name) then
				Hostile = true
				break
			end
		end
	end
	return Hostile or Player.Status.isHostile.Value
end
API.Lag = function()
	if API.GuardsFull() then
		API.GetGun("M9")
	else
		if plr.Team ~= game.Teams.Guards then
			API.SwitchTeam(game.Teams.Guards)
		end
	end
	local Gun = nil
	repeat
		task.wait()
		Gun = plr.Backpack:FindFirstChild("M9") or plr.Character:FindFirstChild("M9") 
	until plr.Backpack:FindFirstChild("M9") or plr.Character:FindFirstChild("M9") 
	game:GetService("ReplicatedStorage").ReloadEvent:FireServer(plr.Backpack:FindFirstChild("M9") or Player.Character:FindFirstChild("M9"))
	local Gun = plr.Backpack:FindFirstChild("M9") or plr.Character:FindFirstChild("M9") 
	task.spawn(function()
		for i = 1,25 do
			game:GetService("ReplicatedStorage").ShootEvent:FireServer({}, Gun)
		end
	end)
end
API.Lag2 = function()
	API.Notif("This might freeze your screen.")
	wait(2)
	if API.GuardsFull() then
		API.GetGun("M9")
	else
		if plr.Team ~= game.Teams.Guards then
			API.SwitchTeam(game.Teams.Guards)
		end
	end
	local Gun = nil
	repeat
		task.wait()
		Gun = plr.Backpack:FindFirstChild("M9") or plr.Character:FindFirstChild("M9") 
	until plr.Backpack:FindFirstChild("M9") or plr.Character:FindFirstChild("M9") 
	game:GetService("ReplicatedStorage").ReloadEvent:FireServer(plr.Backpack:FindFirstChild("M9") or Player.Character:FindFirstChild("M9"))
	local Gun = plr.Backpack:FindFirstChild("M9") or plr.Character:FindFirstChild("M9") 
	task.spawn(function()
		local Ar = {}
		for i =1,55999 do
			Ar[#Ar + 1] = {
				["RayObject"] = Ray.new(Vector3.new(990.272583, 101.489975, 2362.74878), Vector3.new(-799.978333, 0.23157759, -5.88794518)),
				["Distance"] = 198.9905242919922,
				["Cframe"] = CFrame.new(894.362549, 101.288307, 2362.53491, -0.0123058055, 0.00259522465, -0.999920964, 3.63797837e-12, 0.999996722, 0.00259542116, 0.999924302, 3.19387436e-05, -0.0123057645),
				["Hit"] = workspace:FindFirstChildOfClass("Part"),
			}
		end
		for i = 1,10 do
			game:GetService("ReplicatedStorage").ShootEvent:FireServer(Ar, Gun)
		end
	end)
end
API.IsBadArea = function(Player)
	local yes = false
	local a,b =pcall(function()
		local mod = require(game.ReplicatedStorage["Modules_client"]["RegionModule_client"])
		local a = pcall(function()
			if mod.findRegion(Player.Character) then
				mod = mod.findRegion(Player.Character)["Name"]
			end
		end)
		if not a then
			return
		end
		local s = false
		for i, v in pairs(game:GetService("ReplicatedStorage").PermittedRegions:GetChildren()) do
			if v and mod == v.Value then
				yes = false
				s = true
			end
		end
		if not s then
			yes = true
		end
	end)
	return yes
end
function API.AntiPunchC(v2)
	if Toggles.AntiPunch == false then
		return
	end
	pcall(function()
		v2.Character:FindFirstChildOfClass("Humanoid").AnimationPlayed:Connect(function(animationTrack)
			pcall(function()
				if not Settings.Unloaded and Toggles.AntiPunch then
					if animationTrack.Animation.AnimationId == "rbxassetid://484200742" or animationTrack.Animation.AnimationId =="rbxassetid://484926359" then
						if (plr.Character.HumanoidRootPart.Position-v2.Character.HumanoidRootPart.Position).magnitude <3.5 and Toggles.AntiPunch then
							if not table.find(Temp.Whitelisted,v2.Name) then
								for i =1,7 do
									game:GetService("ReplicatedStorage").meleeEvent:FireServer(v2,{Name ='Crude Knife'})
								end
							end
						end
					end
				end
			end)
		end)
	end)
end
function API.GuardsFull(a)
	if #game:GetService("Teams").Guards:GetPlayers()==8 and plr.Team ~= game.Teams.Guards then
		return true
	end
	return false
end

API.Bring = function(Target,TeleportTo)
	local O,O2 = nil,nil
	if Toggles.AntiBring then
		O = true
		Toggles.AntiBring=false
	end
	if Toggles.AntiSit then
		O2 = true
		Toggles.AntiSit=false
	end
	if Target.Character:FindFirstChildOfClass("Humanoid").Sit == true or not Target or Target.Character:FindFirstChildOfClass("Humanoid").Health <1 or plr.Character:FindFirstChildOfClass("Humanoid").Health <1 then
		return API.Notif("Can't bring player right now! (Sitting or Dead)","Error",2)
	end
	if not TeleportTo then TeleportTo = API.GetPos()end
	local Lastpos,CarPad,Car = API.GetPos(),workspace.Prison_ITEMS.buttons["Car Spawner"],nil
	for i,v in pairs(game:GetService("Workspace").CarContainer:GetChildren()) do
		if v then
			if v:FindFirstChild("Body") and v:FindFirstChild("Body"):FindFirstChild("VehicleSeat") and v:FindFirstChild("Body"):FindFirstChild("VehicleSeat").Occupant == nil then
				Car = v
			end
		end
	end
	if not Car then
		coroutine.wrap(function()
			if not Car then
				Car = game:GetService("Workspace").CarContainer.ChildAdded:Wait()
			end
		end)()
		repeat task.wait()
			game:GetService("Players").LocalPlayer.Character:SetPrimaryPartCFrame(CFrame.new(-524, 55, 1777))
			task.spawn(function()
				workspace.Remote.ItemHandler:InvokeServer(game:GetService("Workspace").Prison_ITEMS.buttons:GetChildren()[7]["Car Spawner"])
			end)
			if not Target or not Target.Character:FindFirstChildOfClass("Humanoid") or Target.Character:FindFirstChildOfClass("Humanoid").Health<1 or plr.Character:FindFirstChildOfClass("Humanoid").Health<1 then
				break
			end
		until Car
	end
	plr.Character:FindFirstChildOfClass("Humanoid").Sit = false
	if Car:FindFirstChild("Wheels") then
		Car.Wheels:Destroy()
	end
	Car:WaitForChild("Body"):WaitForChild("VehicleSeat")
	Car.PrimaryPart = Car.Body.VehicleSeat
	Car:SetPrimaryPartCFrame(API.GetPos())
	local Seat = Car.Body.VehicleSeat
	repeat RStep:Wait()
		Seat:Sit(plr.Character:FindFirstChildOfClass("Humanoid"))
	until plr.Character:FindFirstChildOfClass("Humanoid").Sit
	local Offset,Change = CFrame.new(0,0,0),1
	repeat task.wait()
		Car.PrimaryPart = Car.Body.VehicleSeat
		if Change == 1 then
			Offset = CFrame.new(-1,0,-3)
			Change=2
		elseif Change == 2 then
			Offset = CFrame.new(1,0,-8)
			Change=3
		elseif Change == 3 then
			Offset = CFrame.new(1,0,-4)
			Change=4
		elseif Change == 4 then
			Offset = CFrame.new(0,0,-3)
			Change=1
		end
		if Target.Character and Target.Character:FindFirstChildOfClass("Humanoid") and Target.Character:FindFirstChildOfClass("Humanoid").MoveDirection.Magnitude >0 then
			Car:SetPrimaryPartCFrame(Target.Character:GetPrimaryPartCFrame()*CFrame.new(0,-2,-5)*Offset)
		else
			if Target and Target.Character and Target.Character:FindFirstChildOfClass("Humanoid") then
				Car:SetPrimaryPartCFrame(Target.Character:GetPrimaryPartCFrame()*CFrame.new(0,-2,-3.5))
			end
		end
	until Target.Character:FindFirstChildOfClass("Humanoid").Sit == true or not Target or Target.Character:FindFirstChildOfClass("Humanoid").Health <1 or plr.Character:FindFirstChildOfClass("Humanoid").Health <1 or plr.Character:FindFirstChildOfClass("Humanoid").Sit ==false
	Car:PivotTo(TeleportTo)
	wait(.1)
	API.MoveTo(Lastpos)
	if O then
		Toggles.AntiBring=true
	end
	if O2 then
		Toggles.AntiSit=true
	end
end

local function CharacterAdded(char)
	if not Settings.Unloaded then
		local Human = char:WaitForChild("Humanoid")
		Human.BreakJointsOnDeath = false
		if Temp.Running then
			Human.WalkSpeed = 24
		end
		Human:SetStateEnabled(Enum.HumanoidStateType.FallingDown, false)
		Human:SetStateEnabled(Enum.HumanoidStateType.Ragdoll, false)
		local TCon; TCon = Human:GetPropertyChangedSignal("Health"):Connect(function()
			if Human.Health < 2 and Toggles.AutoRespawn then
				TCon:Disconnect()
				API.SwitchTeam(plr.Team, true,true,true)
			end
		end)
		Human.AnimationPlayed:Connect(function(ar)
			if Toggles.AntiArrest and ar.Animation.AnimationId == "rbxassetid://287112271" then
				task.spawn(function()
					local i = Instance.new("Folder",plr.Character)
					i.Name = "WaitingForRespawn"
					repeat RStep:Wait()
						pcall(function()
							if plr.Character:FindFirstChild("WaitingForRespawn") then
								Temp.Pos = plr.Character.HumanoidRootPart.CFrame
							end
						end)
					until not plr.Character or not i or not plr.Character:FindFirstChild("WaitingForRespawn")
					wait()
					API.MoveTo(Temp.Pos)
					wait(.7)
					if (Temp.Pos.Position - plr.Character.HumanoidRootPart.Position).magnitude >30 then
						API.MoveTo(Temp.Pos)
					end
					wait()
					Temp.Pos=nil
				end)
				ar:Stop()
				repeat wait()
					plr.Character:FindFirstChildOfClass("Humanoid").WalkSpeed = 16
				until plr.Character:FindFirstChildOfClass("Humanoid").WalkSpeed >15
			end
			if Toggles.AntiTase and ar.Animation.AnimationId == "rbxassetid://279227693" then
				ar:Stop(); 
				wait()
				repeat wait()
					plr.Character:FindFirstChildOfClass("Humanoid").WalkSpeed = 16
				until plr.Character:FindFirstChildOfClass("Humanoid").WalkSpeed >13
			end
		end)
		if Toggles.AutoItems then
			wait(.3)
			for i =1,2 do
				if game:GetService("MarketplaceService"):UserOwnsGamePassAsync(plr.UserId, 96651) or game:GetService("MarketplaceService"):UserOwnsGamePassAsync(plr.UserId, 643697197) then
					API.GetGun("M4A1")
				end
				API.GetGun("AK-47")
				API.GetGun("Remington 870")
				API.GetGun("M9")
			end
		end
		if Temp.FlyEnabled then
			API.Unfly()
			wait(.4)
			API.Fly(Temp.FlyEnabled)
		end
	end
end
plr.CharacterAdded:Connect(function(char)
	CharacterAdded(char)	
end);

--// Chatting and stuff
plr.Chatted:Connect(function(Chat)
	Chat=Chat:lower()
	if string.sub(Chat,1,1) == Settings.Prefix then
		local Split = Chat:split(" ")
		local IsReal = Script.RunCommand(Split[1],Split,true,Chat)
		if not IsReal then
			API.Notif("No such command as '"..Split[1].."' in the command list")
		end
	end
end)
CommandBar.Changed:Connect(function()
	if string.sub(CommandBar.Text,1,1) == Settings.Prefix or string.sub(CommandBar.Text,1,1) == ";"  then
		CommandBar.Text = string.sub(CommandBar.Text,2,#CommandBar.Text)
	end
end)
CommandBar.FocusLost:Connect(function(Enter)
	if Enter and #CommandBar.Text>0 then
		local Split = CommandBar.Text:split(" ")
		local IsReal = Script.RunCommand(Split[1],Split)
		if not IsReal then
			API.Notif("No such command as '"..Split[1].."' in the command list")
		end
		CommandBar.Text = ""
	end
end)
local LastPosition = API.GetPos()
Temp.MainConnection = game:GetService("RunService").PreRender:Connect(function()
	if not Settings.Unloaded then
		if Toggles.AntiFling then
			pcall(function()
				for i,v in pairs(game:GetService("Players"):GetPlayers()) do
					if v and v ~=plr then
						local PrimaryPart = v.Character.PrimaryPart
						if (v.Character and v.Character:IsDescendantOf(workspace)) and (PrimaryPart and PrimaryPart:IsDescendantOf(v.Character)) then
							if PrimaryPart.AssemblyAngularVelocity.Magnitude > 50 or PrimaryPart.AssemblyLinearVelocity.Magnitude > 100 then
								for i,v in ipairs(v.Character:GetDescendants()) do
									if v:IsA("BasePart") then
										v.CanCollide = false
										v.AssemblyAngularVelocity = Vector3.new(0, 0, 0)
										v.AssemblyLinearVelocity = Vector3.new(0, 0, 0)
										v.CustomPhysicalProperties = PhysicalProperties.new(0, 0, 0)
									end
								end
								PrimaryPart.CanCollide = false
								PrimaryPart.AssemblyAngularVelocity = Vector3.new(0, 0, 0)
								PrimaryPart.AssemblyLinearVelocity = Vector3.new(0, 0, 0)
								PrimaryPart.CustomPhysicalProperties = PhysicalProperties.new(0, 0, 0)
							end
						end
					end
				end
				local PrimaryPart = plr.Character.PrimaryPart
				if PrimaryPart.AssemblyLinearVelocity.Magnitude > 250 or PrimaryPart.AssemblyAngularVelocity.Magnitude > 250 then
					PrimaryPart.AssemblyAngularVelocity = Vector3.new(0, 0, 0)
					PrimaryPart.AssemblyLinearVelocity = Vector3.new(0, 0, 0)
					PrimaryPart.CFrame = LastPosition
				elseif PrimaryPart.AssemblyLinearVelocity.Magnitude < 50 or PrimaryPart.AssemblyAngularVelocity.Magnitude > 50 then
					LastPosition = PrimaryPart.CFrame
				end
			end)
		end
	end
end)
game:GetService("UserInputService").InputBegan:Connect(function(Input,a)
	if not a and not game:GetService("UserInputService"):GetFocusedTextBox() then
		if Input.KeyCode == Enum.KeyCode.LeftShift then
			Temp.Running = true
		end
	end
end)
game:GetService("UserInputService").InputEnded:Connect(function(Input,a)
	if not a and not game:GetService("UserInputService"):GetFocusedTextBox() then
		if Input.KeyCode == Enum.KeyCode.LeftShift then
			Temp.Running = false
		end
		if Input.KeyCode == Enum.KeyCode.Semicolon then
			CommandBar:CaptureFocus()
		end
		if Input.KeyCode == Enum.KeyCode.F and Toggles.OnePunch then
			local a = API.GetPos()
			for i,v in pairs(game:GetService("Players"):GetPlayers()) do
				if v and v~= plr and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
					if (a.Position-plr.Character:FindFirstChild("HumanoidRootPart").Position).Magnitude <3 and not Temp.Running then
						for i =1,7 do
							game:GetService("ReplicatedStorage").meleeEvent:FireServer(v,{Name ='Crude Knife'})
						end
					end
				end
			end
		end
	end
end)
