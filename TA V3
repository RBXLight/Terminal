local GetUrl = function(URL)
	if (http_request or request) then --// Celery is so ass bruh
		return (http_request or request)({
			Url = URL,
			Method = "GET"
		}).Body
	else
		return game:HttpGet(URL)
	end
end


if not firetouchinterest then
	getfenv(1).firetouchinterest = function(TouchWith,Part) --// For bad executors
		local oldc = Part.CFrame
		Part.CFrame=TouchWith.CFrame
		task.wait()
		Part.CFrame = oldc
	end
end
